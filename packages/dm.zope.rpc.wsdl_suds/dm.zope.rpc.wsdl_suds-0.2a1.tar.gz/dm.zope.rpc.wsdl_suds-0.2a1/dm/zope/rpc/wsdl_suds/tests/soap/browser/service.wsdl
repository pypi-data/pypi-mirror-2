<wsdl:definitions
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
  xmlns:tns="http://www.dieter.handshake.de/namespace/dm.zope.rpc/service/0.1a1/"
  xmlns:s="http://www.w3.org/2001/XMLSchema"
  xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  targetNamespace="http://www.dieter.handshake.de/namespace/dm.zope.rpc/service/0.1a1/"
  >

  <wsdl:types>
    <s:schema elementFormDefault="qualified" targetNamespace="http://www.dieter.handshake.de/namespace/dm.zope.rpc/service/0.1a1/">

      <!-- auxiliary type -->
      <s:complexType name="now_and_today">
	<s:sequence>
	  <s:element name="now" type="s:dateTime" />
	  <s:element name="today" type="s:date" />
	</s:sequence>
      </s:complexType>

      <!-- echo1(v1) -> v1 -->
      <s:element name="echo1">
        <s:complexType>
	  <s:sequence>
	    <s:element name="v1" type="s:string" />
	  </s:sequence>
	</s:complexType>
      </s:element>

      <s:element name="echo1Response">
        <s:complexType>
	  <s:sequence>
	    <s:element name="echo1Result" type="s:string" />
	  </s:sequence>
	</s:complexType>
      </s:element>

      <!-- echo2(v1, v2) -> (v1, v2) -->
      <s:element name="echo2">
        <s:complexType>
	  <s:sequence>
	    <s:element name="v1" type="s:string" />
	    <s:element name="v2" type="s:string" />
	  </s:sequence>
	</s:complexType>
      </s:element>

      <s:element name="echo2Response">
        <s:complexType>
	  <s:sequence>
	    <s:element name="v1" type="s:string"/>
	    <s:element name="v2" type="s:string"/>
	  </s:sequence>
	</s:complexType>
      </s:element>

      <!-- exception() -> None -->
      <s:element name="exception">
        <s:complexType>
	  <s:sequence />
        </s:complexType>
      </s:element>
        
      <s:element name="exceptionResponse">
        <s:complexType>
	  <s:sequence />
        </s:complexType>
      </s:element>

      <!-- now_and_today() -> (now, today) -->
      <s:element name="now_and_today">
        <s:complexType>
	  <s:sequence />
        </s:complexType>
      </s:element>
        
      <s:element name="now_and_todayResponse" type="tns:now_and_today" />

      <!-- now_and_today_dict() -> {now, today} -->
      <s:element name="now_and_today_dict">
        <s:complexType>
	  <s:sequence />
        </s:complexType>
      </s:element>
        
      <s:element name="now_and_today_dictResponse" type="tns:now_and_today" />

      <!-- now_and_today_dict_structure() -> {now, today} -->
      <s:element name="now_and_today_dict_structure">
        <s:complexType>
	  <s:sequence />
        </s:complexType>
      </s:element>
        
      <s:element name="now_and_today_dict_structureResponse">
        <s:complexType>
	  <s:sequence>
	    <s:element name="now" type="tns:now_and_today" />
	  </s:sequence>
        </s:complexType>
      </s:element>

    </s:schema>
  </wsdl:types>

  <wsdl:message name="echo1SoapIn">
    <wsdl:part name="parameters" element="tns:echo1" />
  </wsdl:message>

  <wsdl:message name="echo1SoapOut">
    <wsdl:part name="parameters" element="tns:echo1Response" />
  </wsdl:message>

  <wsdl:message name="echo2SoapIn">
    <wsdl:part name="parameters" element="tns:echo2" />
  </wsdl:message>

  <wsdl:message name="echo2SoapOut">
    <wsdl:part name="parameters" element="tns:echo2Response" />
  </wsdl:message>

  <wsdl:message name="exceptionSoapIn">
    <wsdl:part name="parameters" element="tns:exception" />
  </wsdl:message>

  <wsdl:message name="exceptionSoapOut">
    <wsdl:part name="parameters" element="tns:exceptionResponse" />
  </wsdl:message>

  <wsdl:message name="now_and_todaySoapIn">
    <wsdl:part name="parameters" element="tns:now_and_today" />
  </wsdl:message>

  <wsdl:message name="now_and_todaySoapOut">
    <wsdl:part name="parameters" element="tns:now_and_todayResponse" />
  </wsdl:message>

  <wsdl:message name="now_and_today_dictSoapIn">
    <wsdl:part name="parameters" element="tns:now_and_today_dict" />
  </wsdl:message>

  <wsdl:message name="now_and_today_dictSoapOut">
    <wsdl:part name="parameters" element="tns:now_and_today_dictResponse" />
  </wsdl:message>

  <wsdl:message name="now_and_today_dict_structureSoapIn">
    <wsdl:part name="parameters" element="tns:now_and_today_dict_structure" />
  </wsdl:message>

  <wsdl:message name="now_and_today_dict_structureSoapOut">
    <wsdl:part name="parameters" element="tns:now_and_today_dict_structureResponse" />
  </wsdl:message>


  <wsdl:portType name="service">
    <wsdl:operation name="echo1">
      <wsdl:input message="tns:echo1SoapIn" />
      <wsdl:output message="tns:echo1SoapOut" />
    </wsdl:operation>
    <wsdl:operation name="echo2">
      <wsdl:input message="tns:echo2SoapIn" />
      <wsdl:output message="tns:echo2SoapOut" />
    </wsdl:operation>
    <wsdl:operation name="exception">
      <wsdl:input message="tns:exceptionSoapIn" />
      <wsdl:output message="tns:exceptionSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="now_and_today">
      <wsdl:input message="tns:now_and_todaySoapIn" />
      <wsdl:output message="tns:now_and_todaySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="now_and_today_dict">
      <wsdl:input message="tns:now_and_today_dictSoapIn" />
      <wsdl:output message="tns:now_and_today_dictSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="now_and_today_dict_structure">
      <wsdl:input message="tns:now_and_today_dict_structureSoapIn" />
      <wsdl:output message="tns:now_and_today_dict_structureSoapOut" />
    </wsdl:operation>
  </wsdl:portType>

  <wsdl:binding name="service" type="tns:service">
    <soap:binding transport="http://schemas.xmlsoap.org/soap/http" />
    <wsdl:operation name="echo1">
      <soap:operation soapAction="" style="document" />
      <wsdl:input><soap:body use="literal" /></wsdl:input>
      <wsdl:output><soap:body use="literal" /></wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="echo2">
      <soap:operation soapAction="" style="document" />
      <wsdl:input><soap:body use="literal" /></wsdl:input>
      <wsdl:output><soap:body use="literal" /></wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="exception">
      <soap:operation soapAction="" style="document" />
      <wsdl:input><soap:body use="literal" /></wsdl:input>
      <wsdl:output><soap:body use="literal" /></wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="now_and_today">
      <soap:operation soapAction="" style="document" />
      <wsdl:input><soap:body use="literal" /></wsdl:input>
      <wsdl:output><soap:body use="literal" /></wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="now_and_today_dict">
      <soap:operation soapAction="" style="document" />
      <wsdl:input><soap:body use="literal" /></wsdl:input>
      <wsdl:output><soap:body use="literal" /></wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="now_and_today_dict_structure">
      <soap:operation soapAction="" style="document" />
      <wsdl:input><soap:body use="literal" /></wsdl:input>
      <wsdl:output><soap:body use="literal" /></wsdl:output>
    </wsdl:operation>
  </wsdl:binding>

  <wsdl:service name="service">
    <wsdl:port name="service" binding="tns:service">
      <soap:address location="http://localhost:8080/++rpc++soap/"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
