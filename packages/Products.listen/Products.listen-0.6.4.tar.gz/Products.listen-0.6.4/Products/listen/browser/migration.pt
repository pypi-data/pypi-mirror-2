<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">
  <body>
    <div metal:fill-slot="main">
      <div metal:define-macro="body"
	   tal:define="results view/results;
                  num_lists view/num_lists">
	<h3>
          Ran migration on <span tal:replace="num_lists">200</span> lists:
	</h3>
	<table>
          <tr tal:repeat="result results">
	    <td><a tal:attributes="href result/url" tal:content="result/title">list title</a></td>
	    - <td tal:content="result/msg">reason</td>
	  </tr>
	</table>
      </div>
    </div>
  </body>
</html>
