<?xml version="1.0"?>
<!-- This file is used to register new types of portlets. It can also
     be used to register completely new column types. See CMFPlone's version
     of this file for more information.
  -->
<portlets>

  <!-- -*- extra stuff goes here -*- -->
  <assignment name="navigation" category="context" key="/"
    manager="plone.leftcolumn" type="portlets.Navigation" remove="True">
  </assignment>
  <assignment name="navigation" category="context" key="/"
    manager="plone.rightcolumn" type="portlets.Navigation" insert-before="*">
    <property name="topLevel">1</property>
    <property name="ptitle">Navigation</property>
  </assignment>
  <assignment name="actions-portlet-tabs" category="context" key="/" insert-before="*"
    manager="plone.leftcolumn"
    type="collective.portlet.actions.ActionsPortlet" visible="True">
  <property name="category">portal_tabs</property>
  <property name="show_title">True</property>
  <property name="ptitle">Tabs</property>
  <property name="default_icon">action_icon.gif</property>
  <property name="show_icons">True</property>
  <property name="root"/>
 </assignment>
</portlets>