<script type="text/javascript">
			document.ivwRefresh = function(url)
			{
				var ivwImage = document.createElement('IMG');
				ivwImage.src = url + '?r=' + escape(document.referrer) + '&d=' + (Math.random()*100000);
				ivwImage.style.display = 'none';
				ivwImage.id = 'ivwpixel';
				var ivwNode = document.getElementById('ivwpixel');
				if(ivwNode)
				{
					ivwNode.parentNode.replaceChild(ivwImage, ivwNode);
				}
				else
				{
					document.body.appendChild(ivwImage);
				}
			}
		</script>
