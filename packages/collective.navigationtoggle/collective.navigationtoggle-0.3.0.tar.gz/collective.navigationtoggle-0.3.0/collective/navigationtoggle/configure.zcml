<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    i18n_domain="collective.navigationtoggle">

  <five:registerPackage package="." initialize=".initialize" />

  <include package="zope.annotation" />
  <include package="plone.registry" />
  <include package="plone.app.registry" />

  <include package=".browser" />

  <genericsetup:registerProfile
      name="default"
      title="collective.navigationtoggle"
      directory="profiles/default"
      description="Installs the collective.navigationtoggle package"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <genericsetup:registerProfile
      name="uninstall"
      title="collective.navigationtoggle"
      directory="profiles/uninstall"
      description="Remove the collective.navigationtoggle package"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />
  
</configure>
