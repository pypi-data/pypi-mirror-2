<html metal:use-macro="views/standard_macros/page">
  <metal:block fill-slot="style_slot">
    <link
      href="main.css" rel="stylesheet" type="text/css"
        tal:attributes="href context/++resource++main.css" />
  </metal:block>
  
  <body metal:fill-slot="body" border="2" tal:omit-tag="">
    <tal:block define="news view/news">
      <h1 class="abstract" tal:content="structure news/++nsinterface++ng.app.converter.object2psadapter.interfaces.IPropertySheet/title"/>
      <div class="abstract" tal:content="structure news/++nsinterface++ng.app.converter.object2psadapter.interfaces.IPropertySheet/abstract"/>
       
      <tal:block tal:define="pager news/++nsinterface++ng.app.objectqueue.interfaces.IObjectQueue/@@pager; p python:pager.setPagerParameters(revert=False, size=path('context/++registry++main_size:'))">
         <metal:block use-macro="views/ng_skin_macros/values">
            <metal:block fill-slot="title"></metal:block>
         </metal:block>
      </tal:block>
    </tal:block>
  </body>
</html>
