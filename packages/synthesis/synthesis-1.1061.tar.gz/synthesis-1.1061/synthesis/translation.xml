<?xml version="1.0" encoding="UTF-8"?>

<!-- This is an example mapping file.  It maps from element names 
	in one xml file (origin) to element names in another xml file 
	(destination).  No knowledge of XML Schema is needed to do the 
	mapping.  "map:" namespaced elements or attributes refer to the
	destination mapping of the immediately preceding or element or 
	attribute.  For elements, the destination element is a child of
	the "map:" namespaced element.  For attributes, the destination
	attribute name is the value of the "map:" attribute.  That's all
	there is to it!-->

<root xmlns:map='http://alexandriaconsulting.com' xmlns:hmis='http://www.hmis.info/schema/2_8/HUD_HMIS_2_8.xsd' xmlns:silly="http://silly.com">
    <some_element date="hello" map:date="hmis:dateCollected">
    	<client/>
    	<map:client>
    		<hmis:Person/>
    	</map:client>
    	<some_unmapped_element/>
    </some_element>
	<some_other_element>
		<agency/>
		<map:map>
			<Provider/>
		</map:map>
		<client/>
		<map:client>
			<Member/>
		</map:client>
	</some_other_element>
</root>