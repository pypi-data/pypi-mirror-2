<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="yaco.deliverancethemeselector">

    <configlet
        title="Deliverance Theme Selector"
        action_id="yaco.deliverancethemeselector.dtssettings"
        appId="yaco.deliverancethemeselector"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@dts-controlpanel"
        icon_expr="string:${portal_url}/++resource++yaco.deliverancethemeselector.images/icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>



