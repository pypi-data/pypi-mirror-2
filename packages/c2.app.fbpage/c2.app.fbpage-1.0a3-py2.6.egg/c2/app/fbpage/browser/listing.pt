<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
     lang="en"
     metal:use-macro="here/@@fb-base/macros/master"
     i18n:domain="c2.app.fbpage">
<body>
<div metal:fill-slot="main">

  <h2 class="heading" tal:content="context/Title">Title</h2>

      <tal:rep repeat="item view/get_items">
        <h3><a class="contentLink" tal:content="item/Title"
                                       tal:attributes="href item/getURL">Title</a></h3>

                <div class="entrybox">

                        <div class="contentDescription">
                                <p tal:content="item/Description"></p>
                                <div class="textRight">
                                        <a href="#" tal:attributes="href item/getURL" i18n:translate="more">Read more...</a>
                                </div>
                        </div>
                        <div class="visualClear"><!-- --></div>
                </div>
                <div class="visualClear"><!-- --></div>
      </tal:rep>


</div>
</body>
</html>