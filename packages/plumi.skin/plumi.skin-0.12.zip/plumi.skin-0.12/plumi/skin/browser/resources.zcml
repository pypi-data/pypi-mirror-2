  <resource
      file="stylesheets/plumi.css"
      name="plumi.css"
      layer="plumi.skin.browser.interfaces.IPlumiSkinLayer"
      />

  <!-- Image -->
  <resource
      file="images/menu.gif"
      name="menu.gif"
      layer="plumi.skin.browser.interfaces.IPlumiSkinLayer"
      />

  <resource
      file="images/bg.png"
