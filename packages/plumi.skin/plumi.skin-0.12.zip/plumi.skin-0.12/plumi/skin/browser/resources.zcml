      />

  <resource
      file="images/downloads.png"
      name="downloads.png"
      layer="plumi.skin.browser.interfaces.IPlumiSkinLayer"
      />

  <resource
      file="images/vlc_icon.gif"
      name="vlc_icon.gif"
