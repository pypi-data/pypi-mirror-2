<html metal:use-macro="context/@@master/macros/master">

    <metal:slot fill-slot="main"
                tal:define="files view/get_files">

        <ul>
            <li tal:repeat="d files">
                <a tal:attributes="href d/name"
                   tal:content="d/name"
                />
            </li>
        </ul>

    </metal:slot>
</html>
