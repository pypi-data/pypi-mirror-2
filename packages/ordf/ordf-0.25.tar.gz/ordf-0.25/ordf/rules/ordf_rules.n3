@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdfg: <http://www.w3.org/2004/03/trix/rdfg-1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix cs: <http://purl.org/vocab/changeset/schema#> .
@prefix ordf: <http://purl.org/NET/ordf/> .
@prefix orul: <http://purl.org/NET/ordf/rules.n3> .

orul: rdfs:label "Simple ORDF Rules" .

{ ?thing ?prop ?type } => { ?thing a rdfs:Resource } ;
  rdfs:isDefinedBy orul: ;
  rdfs:label "Anything standing as a subject is a Resource. This is to facilitate the default lens" .

{ ?thing rdfs:isDefinedBy ?graph } => { ?graph a rdfg:Graph } ;
  rdfs:isDefinedBy orul: ;
  rdfs:label "Anything that has been defined has been defined in a rdfg:Graph" .
