<configure xmlns="http://namespaces.zope.org/cornerstone">

  <result for="*"
          class=".env.TestResult"
          permission="zope.Public"       
          name='testresult'
          slicesize="20"
          threshold="20">
        
      <batch domain="testbatch"
             vocab=".env.TestBatchVocab"
             template="templates/batch.pt"
             firstpagelink="True"
             lastpagelink="True"
             prevpagelink="True"
             nextpagelink="True"
             batchrange="30" />
           
      <slice class=".env.TestSlice"
             name="testslice"/>

  </result>

</configure>