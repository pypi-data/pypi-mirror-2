a.batchlink:click {
    evt-click-preventdefault: True;
    action-server: renderCornerstoneUiResult;
    renderCornerstoneUiResult-href: nodeAttr(href);
    renderCornerstoneUiResult-selector: kssAttr('selector');
}

a.batchlink img:click {
    evt-click-preventdefault: True;
    action-server: renderCornerstoneUiResult;
    renderCornerstoneUiResult-href: nodeAttr(name);
    renderCornerstoneUiResult-selector: kssAttr('selector');
}