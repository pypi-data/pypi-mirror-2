<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Redaktion Theme">

 <object name="rc_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="rc.theme:skins/rc_theme_custom_images"/>
 <object name="rc_theme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="rc.theme:skins/rc_theme_custom_templates"/>
 <object name="rc_theme_styles"
    meta_type="Filesystem Directory View"
    directory="rc.theme:skins/rc_theme_styles"/>

 <skin-path name="Redaktion Theme" based-on="Plone Default">
  <layer name="rc_theme_custom_images"
     insert-after="custom"/>
  <layer name="rc_theme_custom_templates"
     insert-after="rc_theme_custom_images"/>
  <layer name="rc_theme_styles"
     insert-after="rc_theme_styles"/>



 </skin-path>

</object>
