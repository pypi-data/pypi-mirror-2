<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="ITYOU Blue Theme">
   
 <object name="ityou_bluetheme_images"
    meta_type="Filesystem Directory View"
    directory="ityou.bluetheme:skins/ityou_bluetheme_images"/>
 <object name="ityou_bluetheme_templates"
    meta_type="Filesystem Directory View"
    directory="ityou.bluetheme:skins/ityou_bluetheme_templates"/>
 <object name="ityou_bluetheme_styles"
    meta_type="Filesystem Directory View"
    directory="ityou.bluetheme:skins/ityou_bluetheme_styles"/>

 <skin-path name="ITYOU Blue Theme" based-on="Sunburst Theme">
  <layer name="ityou_bluetheme_images"
     insert-after="custom"/>
  <layer name="ityou_bluetheme_templates"
     insert-after="ityou_bluetheme_images"/>
  <layer name="ityou_bluetheme_styles"
     insert-after="ityou_bluetheme_templates"/>
 </skin-path>

</object>
