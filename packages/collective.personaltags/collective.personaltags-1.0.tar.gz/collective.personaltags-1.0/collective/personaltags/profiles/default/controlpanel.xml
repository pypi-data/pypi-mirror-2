<?xml version="1.0"?>

<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.personaltags"
    purge="False">

    <configlet
        title="Personal Tags"
        action_id="persontaltags"
        appId="collective.personaltags"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@personaltags-controlpanel"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
