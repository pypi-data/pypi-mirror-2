#!/usr/bin/make -f
               
build: build-stamp
	dh build
	touch build-stamp

build-stamp:
	dh_auto_configure

clean:
	rm -rf metamodel.egg-info
	dh clean

install: build install-stamp
install-stamp:
	dh install
	touch install-stamp

binary-arch: install
	dh binary-arch

binary-indep: install
	dh binary-indep

binary: binary-arch binary-indep

