<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="SubscriptionViewer"/>
  </roles>
  <permissions>
    <permission name="PlacelessSubscription: View Content"
                acquire="True">
      <role name="Manager"/>
      <role name="SubscriptionViewer"/>
    </permission>
    <permission name="View" acquire="True" >
      <role name="SubscriptionViewer"/>
    </permission>
  </permissions>
</rolemap>
