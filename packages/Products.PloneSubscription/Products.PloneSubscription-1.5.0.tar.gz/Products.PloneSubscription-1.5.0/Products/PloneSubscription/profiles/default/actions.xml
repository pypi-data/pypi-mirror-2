<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions" />
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="addSubscription" meta_type="CMF Action" i18n:domain="plone">
   <property name="title"
      i18n:translate="">action_add_to_subscriptions</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/addSubscription</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:member and portal.portal_subscription.getShow_document_action() and portal.portal_subscription.getProvider() is not None and not (not portal.portal_subscription.getAuthorize_root_subscription() and (object.portal_type == portal.portal_type or folder.portal_type == portal.portal_type))</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="object" meta_type="CMF Action Category">
  <object name="addGroupSubscription" meta_type="CMF Action"
     i18n:domain="plone">
   <property name="title"
      i18n:translate="">action_add_group_subscriptions</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object_url}/grouped_subscription_add_form</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python: member and portal.portal_subscription.getProvider() is not None</property>
   <property name="permissions">
    <element value="PlacelessSubscription: Manage Content"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="mailing" meta_type="CMF Action" i18n:domain="plone">
   <property name="title"
      i18n:translate="">action_subscription_mailing</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object_url}/subscription_mailing</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python: member and portal.portal_subscription.getProvider() is not None</property>
   <property name="permissions">
    <element value="PlacelessSubscription: Manage Content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="user" meta_type="CMF Action Category">
  <object name="my_subscriptions" meta_type="CMF Action"
     i18n:domain="plonearticle">
   <property name="title" i18n:translate="">action_my_subscriptions</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${portal_url}/my_subscriptions</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:member and portal.portal_subscription.hasSubscription()</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="folder" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="addSubscriptionProvider" meta_type="CMF Action">
   <property name="title">action_add_subscription_provider</property>
   <property name="description"></property>
   <property
      name="url_expr">string:${object_url}/addSubscriptionProvider</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python: portal.portal_subscription.getShow_actions() and 'subscription_provider' not in object.objectIds()</property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="addTemplatesProvider" meta_type="CMF Action">
   <property name="title">action_add_templates_provider</property>
   <property name="description"></property>
   <property
      name="url_expr">string:${object_url}/addTemplatesProvider</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python: portal.portal_subscription.showActions() and 'subscription_templates' not in object.objectIds()</property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object> 
</object>
