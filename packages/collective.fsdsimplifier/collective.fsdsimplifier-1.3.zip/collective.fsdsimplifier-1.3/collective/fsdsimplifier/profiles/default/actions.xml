<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <!-- object actions -->
 <object name="object">
  <object name="folderContents">
   <property
      name="available_expr">python:object.displayContentsTab and not path('object/fsd_simplifier_utils/hide_tabs|nothing')</property>
  </object>
  <object name="local_roles">
   <property
      name="available_expr">not: object/fsd_simplifier_utils/hide_tabs|nothing</property>
  </object>
  <object name="relations">
   <property
      name="available_expr">python:object.relations_adddelete_vocab(test_only=1) and not path('object/fsd_simplifier_utils/hide_tabs|nothing')</property>
  </object>
 </object>
 <!-- user actions -->
 <object name="user">
  <object name="fsdmystuff" meta_type="CMF Action">
   <property name="title" i18n:translate="">My Profile</property>
   <property name="available_expr">python:member is not None and modules['Products.CMFCore.utils'].getToolByName(portal, 'facultystaffdirectory_tool').fsdShowMyFolder() and path('object/fsd_simplifier_utils/is_plone4')</property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
