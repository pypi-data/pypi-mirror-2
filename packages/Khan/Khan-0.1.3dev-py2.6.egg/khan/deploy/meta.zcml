<?xml version="1.0" encoding="UTF-8"?>
<configure xmlns="http://namespaces.zope.org/zope" xmlns:meta="http://namespaces.zope.org/meta"
    xmlns:khan="http://bitbucket.org/khan/khan">
    <meta:directives namespace="http://bitbucket.org/khan/khan">
        <meta:directive name="adapter" schema="repoze.zcml.IAdapterDirective"
            handler="repoze.zcml.adapter" />
        <meta:directive name="subscriber" schema="repoze.zcml.ISubscriberDirective"
            handler="repoze.zcml.subscriber" />
        <meta:complexDirective name="utility"
            schema=".zcml.IUtilityDirective" handler=".zcml.UtilityDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="default"
            schema=".zcml.IDefaultDirective" handler=".zcml.DefaultDirectiveDefinition">
            <meta:subdirective name="var"
                schema=".zcml.IDefaultVarSubDirective" />
        </meta:complexDirective>
        <meta:groupingDirective name="deployment"
            schema=".zcml.IDeploymentDirective" handler=".zcml.DeploymentDirectiveDefinition" />
        <meta:complexDirective name="app"
            schema=".zcml.IAppDirective" handler=".zcml.AppDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="composite"
            schema=".zcml.ICompositeDirective" handler=".zcml.CompositeDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="middleware"
            schema=".zcml.IMiddlewareDirective" handler=".zcml.MiddlewareDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="filter"
            schema=".zcml.IMiddlewareDirective" handler=".zcml.MiddlewareDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="object"
            schema=".zcml.IObjectDirective" handler=".zcml.ObjectDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="server"
            schema=".zcml.IServerDirective" handler=".zcml.ServerDirectiveDefinition">
            <meta:subdirective name="param" schema=".zcml.IParamSubDirective" />
        </meta:complexDirective>
        <meta:complexDirective name="logging"
            schema=".zcml.ILoggingDirective" handler=".zcml.LoggingDirectiveDefinition">
            <meta:subdirective name="logger"
                schema=".zcml.ILoggingLoggerDirective" />
            <meta:subdirective name="handler"
                schema=".zcml.ILoggingHandlerDirective" />
            <meta:subdirective name="formatter"
                schema=".zcml.ILoggingFormatterDirective" />
        </meta:complexDirective>
    </meta:directives>
</configure>
