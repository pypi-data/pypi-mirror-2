<?xml version="1.0"?>
<object>
  <order manager="plone.portalheader" skinname="Laboral theme" based-on="Sunburst Theme">
        <viewlet name="plonetheme.laboral.head" insert-before="*" />
        <viewlet name="plonetheme.laboral.menu" insert-after="plonetheme.laboral.head" />
        <viewlet name="plonetheme.laboral.slider" insert-after="*" />
  </order>
  
  <order manager="plone.portalfooter" skinname="Laboral theme" based-on="Sunburst Theme">
        <viewlet name="plonetheme.laboral.personal_bar" insert-before="*" />
        <viewlet name="plonetheme.laboral.footer" insert-after="plonetheme.laboral.personal_bar" />
  </order>
  
  <order manager="plonetheme.laboral.headManager" skinname="Laboral theme">
        <viewlet name="plonetheme.laboral.languageSelector" />
        <viewlet name="plonetheme.laboral.logo" />
        <viewlet name="plonetheme.laboral.searchbox" />
  </order>
  
  <hidden manager="plone.portalheader" skinname="Laboral theme">
        <viewlet name="plone.skip_links"/>
        <viewlet name="plone.site_actions"/>
        <viewlet name="plone.personal_bar"/>
        <viewlet name="plone.searchbox"/>
        <viewlet name="plone.global_sections"/>
        <viewlet name="plone.logo"/>
        <viewlet name="plone.app.i18n.locales.languageselector"/>
        <viewlet name="plonetheme.laboral.slider" insert-after="*" />
  </hidden>
  
  <hidden manager="plone.abovecontent" skinname="Laboral theme">
        <viewlet name="plone.path_bar"/>
  </hidden>
  
  <hidden manager="plone.belowcontentbody" skinname="Laboral theme">
        <viewlet name="plone.belowcontentbody.relateditems"/>
  </hidden>
  
  <hidden manager="plone.belowcontent" skinname="Laboral theme">
        <viewlet name="plone.belowcontenttitle.keywords"/>
  </hidden>
  
  <hidden manager="plone.portalfooter" skinname="Laboral theme">
        <viewlet name="plone.footer"/>
        <viewlet name="plone.colophon"/>
        <viewlet name="plone.site_actions"/>
  </hidden>
</object>
