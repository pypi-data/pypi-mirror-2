\frametitle {Not surprising}
\begin{lstlisting}
>>> noise = 0.5
>>> t = np.linspace(0,8*pi,1024)

>>> x =  (np.sin(5*t) + np.sin(1.33*t) +
     noise*np.random.randn(t.shape[-1]))

>>> y =  (np.sin(5*t + pi/4) +
          np.sin(1.33*t-pi/2) +
     noise*np.random.randn(t.shape[-1]))
\end{lstlisting}
