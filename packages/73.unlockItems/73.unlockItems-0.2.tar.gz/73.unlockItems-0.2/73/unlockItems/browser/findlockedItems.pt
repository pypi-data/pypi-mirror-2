<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="73.unlockItems">

       <head>
	                <metal:block metal:fill-slot="head_slot">
					<link type="css" rel="stylesheet" href="++resource++73.unlockItems.stylesheets/main.css">
					
			<script type="text/javascript" src="++resource++73.unlockItems.javascript/jqueryUnlockItems.js">
			</script>
			<script type="text/javascript" src="++resource++73.unlockItems.javascript/unlockItems.js">
			</script>
	                </metal:block>
	        </head>

  <metal:block fill-slot="base">
    <tal:block define="dummy python:request.set('disable_border', 1)" />
  </metal:block>

  <body>

    <div metal:fill-slot="main">

<form tal:attributes="action string:./@@findlockedItems" name="findlockeditems_form" method="POST">
		<div tal:define="results view/getLockedItems">
			<h3 tal:condition="results">R&eacute;sultat de la recherche des contenus "Locked"</h3>
			<span  tal:condition="python:not results"><strong>Aucun contenus 'Locked' :-)</strong></span>
			<table id="unlockTable" tal:condition="results">

			    <tr>
			    	<th width="10" NOWRAP><a id="selection-etudiant-all" onClick="javascript:toggleCheck(1)" style="cursor:pointer;">Tous</a>
                            &nbsp;/&nbsp;
	                        <a id="selection-etudiant-none" onClick="javascript:toggleCheck(0)" style="cursor:pointer;">Aucun</a></th>
			    	<th>ID</th>
			    	<th>Titre</th>
			    	<th>Createur</th>
			    	<th>Type</th>
			    	<th>Path</th>
			    </tr>
				<tr tal:repeat="res results">
					<td><input type="checkbox" name="itemId:list" tal:attributes="value res/getId"></td>
					<td><span tal:content="res/getId"/></td>
					<td><span tal:content="res/Title"/></td>
					<td><span tal:content="res/Creator"/></td>
					<td><span tal:content="res/Type"/></td>
					<td><span tal:content="res/getPath"/></td>
				</tr>
				</table>
				<center tal:condition="results"><input type="button" name="button" value="Deverrouiller" onclick="javascript:items2unlock();"/></center>

			
		</div>
</form>
		</div>
	</body>
</html>