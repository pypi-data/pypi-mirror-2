<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="inqbus.collection.proxy">

    <metal:fillbase fill-slot="base">
        <tal:hide condition="nothing">
            <base href="" tal:attributes="href view/render_base" />
        </tal:hide>
    </metal:fillbase>

    <body>
        <div metal:fill-slot="main">
            <tal:main-macro metal:define-macro="main">

                <tal:redirect tal:condition="not:python:mtool.checkPermission('Modify portal content', context)"
                              tal:content="python: request.RESPONSE.redirect('.')" />

                <h1>Content Proxy</h1>
                <p>There are no content found to show up here.</p>
            </tal:main-macro>
        </div>
    </body>
</html>
