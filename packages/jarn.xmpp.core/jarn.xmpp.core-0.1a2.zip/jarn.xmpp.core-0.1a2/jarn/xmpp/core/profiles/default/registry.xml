<?xml version="1.0"?>
<registry>

  <record name="jarn.xmpp.xmppDomain">
    <field type="plone.registry.field.ASCIILine">
      <title>XMPP Domain</title>
    </field>
    <value>myserver</value>
  </record>

  <record name="jarn.xmpp.adminJID">
    <field type="plone.registry.field.ASCIILine">
      <title>Admin JID</title>
    </field>
    <value>admin@myserver</value>
  </record>

  <record name="jarn.xmpp.adminPassword">
    <field type="plone.registry.field.Password">
      <title>Admin password</title>
    </field>
    <value>admin</value>
  </record>

  <record name="jarn.xmpp.pubsubJID">
    <field type="plone.registry.field.ASCIILine">
      <title>PubSub JID</title>
    </field>
    <value>pubsub.myserver</value>
  </record>

  <record name="jarn.xmpp.conferenceJID">
    <field type="plone.registry.field.ASCIILine">
      <title>Conference JID</title>
    </field>
    <value>conference.myserver</value>
  </record>

  <record name="jarn.xmpp.boshURL">
    <field type="plone.registry.field.URI">
      <title>Conference JID</title>
    </field>
    <value>http://myserver/http-bind/</value>
  </record>

</registry>