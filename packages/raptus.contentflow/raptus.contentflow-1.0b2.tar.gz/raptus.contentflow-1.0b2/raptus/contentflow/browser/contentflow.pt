<div xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
     xmlns:tal="http://xml.zope.org/namespaces/tal"
     xmlns:metal="http://xml.zope.org/namespaces/metal"
     xmlns:i18n="http://xml.zope.org/namespaces/i18n"
     id="contentflow">
  <object name="movie" classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=10,0,0,0" width="" height=""
          tal:attributes="width view/width;
                          height view/height;">
    <param name="movie" value=""
           tal:attributes="value string:${context/absolute_url}/++resource++contentflow.swf" />
    <param name="quality" value="high" />
    <param name="menu" value="false" />
    <param name="wmode" wmode="opaque" />
    <param name="FlashVars" value=""
           tal:attributes="value string:xmlPath=${view/xmlPath}&labelColor=${view/labelColor}&backgroundColor=${view/backgroundColor}" />
    <embed name="movie" type="application/x-shockwave-flash" src="" quality="high" menu="0" width="" height="" pluginspage="http://www.macromedia.com/go/getflashplayer"
           tal:attributes="width view/width;
                           height view/height;
                           FlashVars string:xmlPath=${view/xmlPath}&labelColor=${view/labelColor}&backgroundColor=${view/backgroundColor};
                           src string:${context/absolute_url}/++resource++contentflow.swf"></embed>
  </object>
</div>