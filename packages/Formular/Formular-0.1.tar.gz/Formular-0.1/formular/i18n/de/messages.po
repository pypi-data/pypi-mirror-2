#: formular/fields.py:91
msgid "The value is invalid."
msgstr "Der Wert ist ungültig."

#: formular/fields.py:92
msgid "This field is required."
msgstr "Dieses Feld wird benötigt."

#: formular/fields.py:93
msgid "Requires {{form.fields[{initial_value}].label}}."
msgstr "Benötigt {{form.fields[{init_value}].label}}"

#: formular/fields.py:94
msgid "Does not equal {{form.fields[{initial_value}].label}}."
msgstr "Gleicht nicht {{form.fields[{init_value}].label}}."

#: formular/fields.py:433
msgid "Must be at least {initial_value} long."
msgstr "Muss zumindest {initial_value} lang sein."

#: formular/fields.py:434
msgid "Must be shorter then {initial_value}."
msgstr "Muss kürzer sein als {initial_value}."

#: formular/fields.py:442
msgid "Must be at least {initial_value} characters long."
msgstr "Muss mindestens {initial_value} Buchstaben lang sein."

#: formular/fields.py:443
msgid "Must be shorter then {initial_value} characters."
msgstr "Muss kürzer als {initial_value} Buchstaben sein."

#: formular/fields.py:466
msgid "Must be greater then {initial_value}."
msgstr "Muss größer als {initial_value} sein."

#: formular/fields.py:467
msgid "Must be lesser then {initial_value}."
msgstr "Muss kleiner als {initial_value} sein."

#: formular/fields.py:481
msgid "A number is required."
msgstr "Eine Zahl wird benötigt."

#: formular/fields.py:497
msgid "An integer is required."
msgstr "Eine Ganzzahl wird benötigt."

#: formular/fields.py:507
msgid "A float is required."
msgstr "Eine Dezimalzahl wird benötigt."

#: formular/fields.py:517
msgid "A decimal is required."
msgstr "Eine Dezimalzahl wird benötigt."

#: formular/fields.py:525
msgid "The value must be either \"True\" or \"False\"."
msgstr "Der Wert muss \"Richtig\" oder \"Falsch\" sein."

#: formular/fields.py:553
msgid "The choice is invalid."
msgstr "Die Wahl is ungültig."

#: formular/fields.py:579
msgid "One or more choices are invalid."
msgstr "Eine oder mehrere Wahlen sind ungültig."

#: formular/fields.py:615
msgid "Requires at least {initial_value} values."
msgstr "Benötigt zumindest {initial_value} Werte."

#: formular/fields.py:616
msgid "Requires at most {initial_value} values."
msgstr "Benötigt weniger als {initial_value} Werte."

#: formular/fields.py:734
msgid "Invalid time."
msgstr "Eine Zeitangabe wird benötigt."

#: formular/fields.py:772
msgid "Invalid date."
msgstr "Eine Datumsangabe wird benötigt."

#: formular/form.py:218
msgid "Form submitted multiple times or session expired. Try again."
msgstr ""
"Das Formular wurde mehrmals gesendet oder die Sitzung ist abgelaufen. "
"Probieren Sie es erneut."
