<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

  <head><title></title></head>

  <body>
    <metal:fill fill-slot="main">
      <metal:main_macro>
        <div tal:condition="python: request.SESSION.has_key('pfg.verkkomaksut')"
             tal:define="uid request/SESSION/pfg.verkkomaksut/ADAPTER_UID">
          <div tal:define="adapter python: context.reference_catalog.lookupObject(uid)">
<!--            <p tal:content="adapter/getThanks_text" />-->
            <div tal:replace="structure adapter/getThanks_text" />
          </div>
        </div>
        <div tal:condition="python: not request.SESSION.has_key('pfg.verkkomaksut')">
          The session has timed out.
        </div>

      </metal:main_macro>
    </metal:fill>
  </body>
</html>
