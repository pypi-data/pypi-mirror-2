<dl class="portlet" id="portlet-zennavigation">

   <dt class="portletHeader">
     <span class="portletTopLeft"></span>
     <a href="#"
           class="tile"
           tal:condition="view/title"
           tal:attributes="href string:${view/site_url}/zentinel"
           tal:content="view/title">Zentinel</a>
     <span class="portletTopRight"></span>
   </dt>

   <dd class="portletItem lastItem"
       tal:define="base view/base;
                   enterprise view/enterprise;
                   additional view/additional">

        <div class="zenNavHeader">Main Views</div>
        <ul class="navTree navTreeLevel0">

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Dashboard">Dashboard</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/viewEvents?notabs=1">Event Console</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/deviceList?notabs=1">Device List</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/viewNetworkMap">Network Map</a>
          </li>

          <li tal:repeat="item python:additional.get('main', [])">
              <a href="" tal:attributes="href string:${base}/${item/link}" tal:content="item/title"/>
          </li>

        </ul>

        <div class="zenNavHeader">Classes</div>
        <ul class="navTree navTreeLevel0">

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Events">Events</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Devices">Devices</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Services">Services</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Processes">Processes</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Manufacturers">Products</a>
          </li>

          <li tal:repeat="item python:additional.get('classes', [])">
              <a href="" tal:attributes="href string:${base}/${item/link}" tal:content="item/title"/>
          </li>
        </ul>

        <div class="zenNavHeader">Browse By</div>
        <ul class="navTree navTreeLevel0">

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Systems">Systems</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Groups">Groups</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Locations">Locations</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Networks">Networks</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Reports">Reports</a>
          </li>

          <li tal:repeat="item python:additional.get('browseby', [])">
              <a href="" tal:attributes="href string:${base}/${item/link}" tal:content="item/title"/>
          </li>
        </ul>

        <div class="zenNavHeader">Management</div>
        <ul class="navTree navTreeLevel0">

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/addDevice">Add Device</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Mibs">MIBs</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/Monitors">Collectors</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/editSettings">Settings</a>
          </li>

          <li class="navTreeItem visualNoMarker">
              <a href="" tal:attributes="href string:${base}/ZenEventManager">Event Manager</a>
          </li>

          <li tal:repeat="item python:additional.get('mgmt', [])">
              <a href="" tal:attributes="href string:${base}/${item/link}" tal:content="item/title"/>
          </li>

        </ul>

    </dd>

    <span class="portletBottomLeft"></span>
    <span class="portletBottomRight"></span>

</dl>
