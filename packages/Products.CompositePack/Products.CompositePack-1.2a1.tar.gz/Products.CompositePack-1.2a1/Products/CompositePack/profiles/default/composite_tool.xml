<?xml version="1.0"?>
<object name="composite_tool" meta_type="CompositePack Tool">
 <composites name="composites">
  <composite name="Navigation Page">
   <c_layout name="two_slots"/>
   <c_layout name="three_slots"/>
   <c_layout name="two_columns" default="True"/>
  </composite>
 </composites>
 <composables name="composites">
  <composable name="(Default Setup)">
   <c_viewlet name="title_description_with_link"/>
  </composable>
  <composable name="CompositePack Fragments">
   <c_viewlet name="fragment_viewlet"/>
  </composable>
  <composable name="CompositePack Titles">
   <c_viewlet name="title_viewlet"/>
  </composable>
  <composable name="Document">
   <c_viewlet name="title_description_with_link"/>
  </composable>
  <composable name="Event">
   <c_viewlet name="title_description_with_link"/>
  </composable>
  <composable name="File">
   <c_viewlet name="title_description_with_link"/>
  </composable>
  <composable name="Image">
   <c_viewlet name="image_viewlet" default="True"/>
   <c_viewlet name="link_viewlet"/>
   <c_viewlet name="image_title_viewlet"/>
   <c_viewlet name="image_caption_viewlet"/>
  </composable>
  <composable name="Link">
   <c_viewlet name="title_description_with_link"/>
  </composable>
  <composable name="News Item">
   <c_viewlet name="title_description_with_link"/>
  </composable>
  <composable name="Topic">
   <c_viewlet name="topic_viewlet"/>
  </composable>
 </composables>
 <viewlets name="viewlets">
  <viewlet name="title_description_with_link" title="Title with description"
     skin_method="title_description_with_link"/>
  <viewlet name="link_viewlet" title="Link Only" skin_method="link_viewlet"/>
  <viewlet name="title_viewlet" title="Title" skin_method="title_viewlet"/>
  <viewlet name="fragment_viewlet" title="HTML Fragment"
     skin_method="fragment_viewlet"/>
  <viewlet name="title_date_viewlet" title="Title and Date"
     skin_method="title_date_viewlet"/>
  <viewlet name="image_viewlet" title="Image" skin_method="image_viewlet"/>
  <viewlet name="image_title_viewlet" title="Image with title"
     skin_method="image_title_viewlet"/>
  <viewlet name="image_caption_viewlet" title="Image with caption"
     skin_method="image_caption_viewlet"/>
 <viewlet name="topic_viewlet" title="Topic Listing" skin_method="topic_viewlet"/>
 </viewlets>
 <layouts name="layouts">
  <layout name="two_slots" title="Two slots" skin_method="two_slots"/>
  <layout name="three_slots" title="Three slots" skin_method="three_slots"/>
  <layout name="two_columns" title="Two columns" skin_method="two_columns"
      default="True"/>
 </layouts>
</object>
