<html>

    <metal:header define-macro="header_resources">
		<metal:header use-macro="context/compo_azax_macros/macros/base_resources" />
		<metal:header use-macro="context/compo_azax_macros/macros/kukit_resource" />
    </metal:header>

    <metal:header define-macro="base_resources">
	<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++sarissa.js;"
      src="++resource++sarissa.js">
    </script>
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++cssQuery.js;"
      src="++resource++cssQuery.js">
    </script>
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++MochiKit.js;"
      src="++resource++MochiKit.js">
    </script>
    <tal:comment condition="nothing"><!-- This may be needed for later versions of kukit.
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++event-registration.js;"
      src="++resource++event-registration.js">
    </script>
    --></tal:comment>
    </metal:header>

	<metal:header define-macro="kukit_resource">
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++kukit.js;"
      src="++resource++kukit.js">
    </script>
    </metal:header>

    <!-- prototype -->

    <metal:header define-macro="header_resources_with_prototype">
		<metal:header use-macro="context/compo_azax_macros/macros/base_resources" />
		<metal:header use-macro="context/compo_azax_macros/macros/prototype_extra_resources" />
		<metal:header use-macro="context/compo_azax_macros/macros/kukit_resource" />
    </metal:header>

    <metal:header define-macro="prototype_extra_resources">
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++prototype.js;"
      src="++resource++prototype.js">
    </script>
    <script type="text/javascript"
      tal:attributes="src string:${context/absolute_url}/cp_container/++resource++effects.js;"
      src="++resource++effects.js">
    </script>
    </metal:header>

    <!-- ajaxslt-->

    <metal:header define-macro="header_resources_with_ajaxslt">
		<metal:header use-macro="context/compo_azax_macros/macros/base_resources" />
		<metal:header use-macro="context/compo_azax_macros/macros/ajaxslt_extra_resources" />
		<metal:header use-macro="context/compo_azax_macros/macros/kukit_resource" />
    </metal:header>

    <metal:header define-macro="ajaxslt_extra_resources">
    <script type="text/javascript"
      tal:attributes="src
      string:${context/absolute_url}/cp_container/++resource++ajaxslt_misc.js;"
      src="++resource++ajaxslt_misc.js">
    </script>
    <script type="text/javascript"
      tal:attributes="src
      string:${context/absolute_url}/cp_container/++resource++ajaxslt_dom.js;"
      src="++resource++ajaxslt_dom.js">
    </script>
    <script type="text/javascript"
      tal:attributes="src
      string:${context/absolute_url}/cp_container/++resource++ajaxslt_xpath.js;"
      src="++resource++ajaxslt_xpath.js">
    </script>
    <!--script type="text/javascript"
      tal:attributes="src
      string:${context/absolute_url}/++resource++ajaxslt_xslt.js;"
      src="++resource++ajaxslt_xslt.js">
    </script-->
    </metal:header>
</html>    
