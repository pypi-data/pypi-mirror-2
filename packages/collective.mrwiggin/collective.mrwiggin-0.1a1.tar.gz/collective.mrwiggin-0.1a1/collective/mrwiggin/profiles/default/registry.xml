<?xml version="1.0"?>
<registry>


    <record name="collective.mrwiggin.layouts">
        <field type="plone.registry.field.Dict">
            <title>Layouts</title>
            <key_type type="plone.registry.field.ASCIILine">
                <title>Key</title>
            </key_type>
            <value_type type="plone.registry.field.ASCIILine">
                <title>Value</title>
            </value_type>
        </field>
        <value>
            <element key="collective.mrwiggin.mainlayout">Products.CMFPlone:skins/plone_templates/main_template.pt</element>
        </value>
    </record>

    <record name="collective.mrwiggin.assigments">
        <field type="plone.registry.field.Dict">
            <title>Assigments</title>
            <key_type type="plone.registry.field.ASCIILine">
                <title>Key</title>
            </key_type>
            <value_type type="plone.registry.field.ASCIILine">
                <title>Value</title>
            </value_type>
        </field>
        <value>
            <element key="/">collective.mrwiggin.mainlayout</element>
        </value>
    </record>

    <records interface="collective.mrwiggin.mainlayout" />
    <record name="collective.mrwiggin.mainlayout.title">
        <value>Default Plone Layout</value>
    </record>
    <record name="collective.mrwiggin.mainlayout.icon">
        <value>++resource++main_layout_icon.png</value>
    </record>
    <record name="collective.mrwiggin.mainlayout.image">
        <value>++resource++main_layout.png</value>
    </record>

</registry>
