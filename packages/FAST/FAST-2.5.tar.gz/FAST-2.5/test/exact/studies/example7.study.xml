<experimental-study name="example7">
  <!--
	An illustration of how an analysis can reference an 
	external results file.
  -->

  <tags>
    <tag>example</tag>
  </tags>

  <analysis name="Validation" type="validation">
    <data experiment="foo" import="example6.study.xml"/>
    <options>_measurement=Value _optimum=100.0 _tolerance=10.0</options>
  </analysis>

</experimental-study>
