<experimental-study name="example19">
  <!--
	A simple experiment with baseline analyses:
		Using a user-specified experimental results file.
  -->

  <tags>
    <tag>example</tag>
  </tags>

  <experiment name="foo">
    <factors>
      <factor name="proby">
        <level name="low">0.1</level>
        <level name="high">0.9</level>
      </factor>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable>./testfn.script</executable>
       <replication>1
	  <seeds>1008734987</seeds>
       </replication>
    </controls>
  </experiment>

  <experiment name="bar">
    <factors>
      <factor name="proby">
        <level name="low">0.4</level>
        <level name="high">0.6</level>
      </factor>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable>./testfn.script</executable>
       <replication>1
	  <seeds>1008734987</seeds>
       </replication>
    </controls>
  </experiment>

  <analysis name="Validation" type="validation">
    <data experiment="exp1" import="example2.study.xml" results="example2-bogus.exp1.results.xml"/>
    <options>_measurement=Value _value=-1.0 _tolerance=0.0 _cmp_operator=eq</options>
  </analysis>

  <!-- The first experiment in the baseline is the one compared
       with all of the other sets of data.  Thus, you can baseline more than
       one experimental result!
  -->
  <analysis name="Baseline1" type="baseline">
    <data experiment="exp1" import="example2.study.xml" results="example2-bogus.exp1.results.xml"/>
    <data experiment="foo"/>
    <data experiment="bar"/>
    <options>_measurement=Value _tolerance=0.0</options>
  </analysis>

</experimental-study>
