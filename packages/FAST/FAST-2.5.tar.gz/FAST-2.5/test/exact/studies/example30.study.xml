<experimental-study name="example30">
  <!--
	A simple experiment that uses a baseline analysis
		with a user-specified comparison function.
		In this case, the comparison function always
		returns "Fail".
  -->

  <tags>
    <tag>example</tag>
  </tags>

  <experiment name="foo">
    <factors>
      <factor name="proby">
        <level name="low">0.1</level>
        <level name="high">0.9</level>
      </factor>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable>./testfn.script</executable>
       <replication>1
	  <seeds>1008734987</seeds>
       </replication>
    </controls>
  </experiment>

  <!-- The first experiment in the baseline is the one compared
       with all of the other sets of data.  Thus, you can baseline more than
       one experimental result!
  -->
  <analysis name="Baseline1" type="baseline">
    <data experiment="exp1" import="example2.study.xml"/>
    <data experiment="foo"/>
    <options>_measurement=Value _tolerance=0.0 _validation_method=example30.validation_fn</options>
    <python>example30</python>
  </analysis>

</experimental-study>
