<experimental-study name="example11">
  <!-- DISABLED -->
  <!--
	A test of the "default" driver compared with the "runexp_simple"
	driver.
  -->

  <tags>
    <tag>example</tag>
  </tags>

  <experiment name="default">
    <factors>
      <factor name="proby">
        <level name="low">0.1</level>
        <level name="high">0.9</level>
      </factor>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable>./testfn.script</executable>
       <replication>5
         <seeds>1008734987 10198098 10289347 103342333 10498098</seeds>
       </replication>
    </controls>
  </experiment>

  <experiment name="simple">
    <factors>
      <factor name="proby">
        <level name="low">0.1</level>
        <level name="high">0.9</level>
      </factor>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable type="runexp_simple">./testfn.script</executable>
       <replication>5
         <seeds>1008734987 10198098 10289347 103342333 10498098</seeds>
       </replication>
    </controls>
  </experiment>

  <analysis name="Baseline1" type="baseline">
    <data experiment="simple"/>
    <data experiment="default"/>
    <options>_measurement=Value</options>
  </analysis>

</experimental-study>
