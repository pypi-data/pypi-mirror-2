<experimental-study name="example3">
  <!--
	A simple experiment using data input from a file.
  -->

  <tags>
    <tag>example</tag>
  </tags>

  <experiment>
    <factors>
      <factor name="proby" filename="example3.info"/>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable>./testfn.script</executable>
       <replication>1
          <seeds>1008734987</seeds>
       </replication>
    </controls>
  </experiment>

  <analysis name="Validation" type="validation">
    <options>_measurement=Value _optimum=100.0 _tolerance=10.0</options>
  </analysis>

</experimental-study>
