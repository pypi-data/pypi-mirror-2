<experimental-study name="example20">
  <!--
	A simple experiment using the timing driver.
  -->

  <tags>
    <tag>example</tag>
  </tags>

  <experiment>
    <factors>
      <factor name="proby">
        <level name="low">0.1</level>
        <level name="high">0.9</level>
      </factor>
      <factor name="scale">
        <level name="low">1.0</level>
        <level name="high">10.0</level>
      </factor>
      <factor name="mapping">
        <level name="log">log</level>
        <level name="exp">exp</level>
      </factor>
    </factors>
    <controls>
       <executable driver="exact_timing">./testfn.script</executable>
       <replication>1
          <seeds>1008734987</seeds>
       </replication>
    </controls>
  </experiment>

  <analysis name="Validation" type="validation">
    <options>_measurement=Value _optimum=100.0 _tolerance=10.0</options>
  </analysis>

  <analysis name="Timing" type="validation">
    <options>_measurement="CPU TotalTime" _value=0 _cmp_operator=gt</options>
  </analysis>

</experimental-study>
