<?xml version="1.0"?>
<object name="portal_skins" request_varname="HTTP_PLONE_SKIN">

 <object name="example_rtsubsites_theme_custom_images"
    meta_type="Filesystem Directory View"
    directory="example.rtsubsites_theme:skins/example_rtsubsites_theme_custom_images"/>
 <object name="example_rtsubsites_theme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="example.rtsubsites_theme:skins/example_rtsubsites_theme_custom_templates"/>
 <object name="example_rtsubsites_theme_styles"
    meta_type="Filesystem Directory View"
    directory="example.rtsubsites_theme:skins/example_rtsubsites_theme_styles"/>

 <skin-path name="Example Theme for redturtle.subsites" based-on="Sunburst Theme">
  <layer name="example_rtsubsites_theme_custom_images"
     insert-after="custom"/>
  <layer name="example_rtsubsites_theme_custom_templates"
     insert-after="example_rtsubsites_theme_custom_images"/>
  <layer name="example_rtsubsites_theme_styles"
     insert-after="example_rtsubsites_theme_custom_templates"/>
 </skin-path>

</object>
