<?xml version="1.0"?>
<object name="Inscricoes" meta_type="Factory-based Type Information"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="Products.PyConBrasil">
 <property name="title" i18n:translate="">Inscricoes</property>
 <property
    name="description" i18n:translate="">Pasta que contem todas as inscricoes de participantes,
      palestras, treinamentos, etc...
      </property>
 <property name="content_icon">inscricoes_icon.gif</property>
 <property name="content_meta_type">Inscricoes</property>
 <property name="product">PyConBrasil</property>
 <property name="factory">addInscricoes</property>
 <property name="immediate_view">base_view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="Inscricao"/>
  <element value="InscricaoCorporativa"/>
  <element value="Treinamento"/>
  <element value="Palestra"/>
  <element value="PalestraRelampago"/>
  <element value="Imprensa"/>
 </property>
 <property name="allow_discussion">True</property>
 <alias from="edit" to="base_edit"/>
 <alias from="(Default)" to="inscricoes_view"/>
 <alias from="view" to="inscricoes_view"/>
 <action title="View" action_id="view" category="object"
    condition_expr="python:1" url_expr="string:${object_url}/inscricoes_view"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/base_edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="Folder Listing" action_id="folderlisting" category="folder"
    condition_expr="object/isPrincipiaFolderish"
    url_expr="string:${folder_url}/view" visible="False">
  <permission value="View"/>
 </action>
 <action title="Inscricoes" action_id="inscricao_list" category="object"
    condition_expr="python:1" url_expr="string:${object_url}/inscricao_list"
    visible="True">
  <permission value="Review portal content"/>
 </action>
 <action title="Palestras" action_id="palestra_list" category="object"
    condition_expr="python:1" url_expr="string:${object_url}/palestra_list"
    visible="True">
  <permission value="Review portal content"/>
 </action>
 <action title="Imprensa" action_id="imprensa_list" category="object"
    condition_expr="python:1" url_expr="string:${object_url}/imprensa_list"
    visible="True">
  <permission value="Review portal content"/>
 </action>
</object>
