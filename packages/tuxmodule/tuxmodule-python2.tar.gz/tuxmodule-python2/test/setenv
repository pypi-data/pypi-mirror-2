# TUXDIR should already be set during build

APPDIR=`pwd`
TUXCONFIG=$APPDIR/TUXCONFIG
QMCONFIG=$APPDIR/QFS
WSNADDR=//localhost:7766
PATH=$TUXDIR/bin:$PATH
LD_LIBRARY_PATH=$TUXDIR/lib:$LD_LIBRARY_PATH

export TUXDIR APPDIR TUXCONFIG QMCONFIG WSNADDR
FIELDTBLS32=tpadm,Usysfl32,dmadmin,evt_mib,mydef
FLDTBLDIR32=$TUXDIR/udataobj:.
export FIELDTBLS32 FLDTBLDIR32

PATH=$TUXDIR/bin:$PATH
LD_LIBRARY_PATH=$TUXDIR/lib:$LD_LIBRARY_PATH
export PATH LD_LIBRARY_PATH

