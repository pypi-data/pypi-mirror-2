<tal:bannerrotation condition="view/enabled|nothing">
    <!-- include Cycle plugin -->
    <script type="text/javascript" tal:attributes="src view/load_jq_cycle"></script>
    <tal:imagearray replace="structure view/get_banner_array"></tal:imagearray>
    <tal:script replace="structure view/get_cycle_script"></tal:script>
    <div class="bannerrotation">
        <img tal:attributes="src view/firstimage" class="first_banner" />
    </div>
</tal:bannerrotation>