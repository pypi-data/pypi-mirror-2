<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="medialog.moreviews">

<metal:css fill-slot="style_slot">
	<tal:replace tal:replace="structure context/galleryview/displayer/css" />
</metal:css>
<metal:js fill-slot="javascript_head_slot">
	<tal:replace tal:replace="structure context/galleryview/displayer/javascript" />
</metal:js>

<body>
<metal:main fill-slot="content-core">
<tal:main-macro metal:define-macro="main">
     
 
	<tal:display tal:condition="python: context.galleryview.adapter.number_of_images > 0" tal:content="structure view/displayer/content" />

	<tal:batch tal:condition="conbtext/gallerview/is_batch">
		<tal:def tal:define="batch context/galleryview/displayer/batch">
    	<div metal:use-macro="context/galleryview/context/batch_macros/macros/navigation">
				Navigation
  		</div>
		</tal:def>
  </tal:batch>
 
</tal:main-macro>
</metal:main>
</body>
</html>

   