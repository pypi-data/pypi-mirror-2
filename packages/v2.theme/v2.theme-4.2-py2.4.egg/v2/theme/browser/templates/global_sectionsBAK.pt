 <ul id="portal-globalnav">
	<li>
		<a href="#" tal:attributes="href root/V2_website/absolute_url" class="plain">home</a>
	</li>
	<span tal:repeat="item root/V2_website/objectValues" tal:omit-tag="">
	<span tal:condition="python:(item.meta_type == 'ATFolder' or item.meta_type == 'ATBTreeFolder' or item.meta_type == 'ATTopic' or item.meta_type == 'ATDocument') and (root.V2_website.portal_workflow.getInfoFor(item, 'review_state') == 'published')" tal:omit-tag="">
	<li tal:condition="not:item/exclude_from_nav">
		<a href="#" tal:attributes="href item/absolute_url" tal:content="item/title" class="plain"> menu item </a>
		<ul class="submenu" tal:condition="python:len(item.objectValues('ATFolder')) + len(item.objectValues('ATBTreeFolder')) + len(item.objectValues('ATTopic')) + len(item.objectValues('ATDocument')) != 0">
			<span tal:repeat="subitem item/objectValues" tal:omit-tag="">
			<span tal:condition="python:(subitem.meta_type == 'ATFolder' or subitem.meta_type == 'ATBTreeFolder' or subitem.meta_type == 'ATTopic' or subitem.meta_type == 'ATDocument') and root.V2_website.portal_workflow.getInfoFor(subitem, 'review_state') == 'published'" tal:omit-tag="">
			<li tal:condition="not:subitem/exclude_from_nav">
				<a href="#" tal:attributes="href subitem/absolute_url" tal:content="subitem/title" class="plain"> menu item </a>
			</li>
			</span>
			</span>
		</ul>
	</li>
	</span>
	</span>
</ul> 




