	border:1px solid #D0D0D0;
	margin-top: 33px;
	margin-left:6px;
	margin-right:6px;
	margin-bottom:6px;
	padding:12px;
	opacity:0.8;
}

.pageWrapper table.listing{
	float: right;
}

.totalWidthTable{
	width:100%;
}

.totalWidthTable td{
	vertical-align: top;
}

/* Links with differently colored link underlines - only for content */
