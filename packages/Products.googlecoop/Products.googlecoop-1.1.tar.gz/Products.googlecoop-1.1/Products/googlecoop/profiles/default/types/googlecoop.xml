<?xml version="1.0"?>
<object name="googlecoop"
   meta_type="Factory-based Type Information"
   i18n:domain="Products.googlecoop" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">Google Coop</property>
  <property name="content_meta_type">googlecoop</property>
  <property name="content_icon">document_icon.gif</property>
  <property name="product">googlecoop</property>
  <property name="factory">addgooglecoop</property>
  <property name="immediate_view">base_view</property>
  <property name="global_allow">False</property>
  <property name="filter_content_types">False</property>
  <property name="allowed_content_types" />
  <property name="allow_discussion">False</property>
  <alias from="(Default)" to="base_view" />
  <alias from="edit" to="base_edit" />
  <alias from="index.html" to="(Default)" />
  <alias from="properties" to="base_metadata" />
  <alias from="sharing" to="@@sharing" />
  <alias from="view" to="base_view" />
  <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/view" visible="True">
    <permission value="View" />
  </action>
  <action title="Edit" action_id="edit" category="object" condition_expr="not:object/@@plone_lock_info/is_locked_for_current_user"
    url_expr="string:${object_url}/edit" visible="True">
    <permission value="Modify portal content" />
  </action>
  <action title="Properties" action_id="metadata" category="object"
    condition_expr="" url_expr="string:${object_url}/properties" visible="True"
    i18n:attributes="">
    <permission value="Modify portal content"/>
  </action>
  <action title="Get all URLs" action_id="getRemoteUrls" category="object"
    condition_expr="" url_expr="string:${object_url}/getAllRemoteURLs"
    visible="True" i18n:attributes="">
    <permission value="View"/>
  </action>
  <action title="Search" action_id="gcSearchForm" category="object"
    condition_expr="" url_expr="string:${object_url}/googlecoop"
    visible="True" i18n:attributes="">
    <permission value="View"/>
  </action>
  <action title="Context XML" action_id="getCseContext" category="object"
    condition_expr="" url_expr="string:${object_url}/cse-context.xml"
    visible="True" i18n:attributes="">
    <permission value="View"/>
  </action>
</object>
