<?xml version="1.0"?>
<object name="MemberProfile"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="betahaus.memberprofile" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">MemberProfile</property>
  <property name="description"
    i18n:translate="">Member profile content type</property>
  <property name="content_meta_type">MemberProfile</property>
  <property name="content_icon">user.gif</property>
  <property name="product">betahaus.memberprofile</property>
  <property name="factory">addMemberProfile</property>
  <property name="immediate_view">atct_edit</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">False</property>
  <property name="allow_discussion">False</property>
  <property name="default_view">base_view</property>
  <property name="view_methods">
    <element value="base_view" />
  </property>
  <alias from="(Default)" to="(dynamic view)" />
  <alias from="edit" to="atct_edit" />
  <alias from="sharing" to="@@sharing" />
  <alias from="view" to="base_view" />
  <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/" visible="True">
    <permission value="View" />
  </action>
  <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
    <permission value="Modify portal content" />
  </action>
  <!-- Not used - for now
  <action title="Forward" action_id="forward" category="object" condition_expr=""
    url_expr="string:${object_url}/forward" visible="True">
    <permission value="View" />
  </action>
  -->
</object>
