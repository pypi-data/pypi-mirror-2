# -*- coding: utf-8 -*-
<%inherit file="/core/hide-right.mako"/>
<%def name="header()">
    % if c.title is not UNDEFINED:
    <title>${c.title or 'Index'}</title>
    % else:
    <title>{{package}} :: Entity</title>
    % endif
    <link type="text/css" rel="stylesheet" href="/css/dashboard.css" media="screen" />
    <link type="text/css" rel="stylesheet" href="/css/fagrid.css" media="screen" />
</%def>
<%def name="content()">
    <h1>${c.title}</h1>
    <table class="fagrid">
    ${c.grid.render()|n}
    </table>
</%def>
<%def name="lhcolumn()">
    <%include file="adminnav.mako" />
    <%include file="motd.mako" />
</%def>
