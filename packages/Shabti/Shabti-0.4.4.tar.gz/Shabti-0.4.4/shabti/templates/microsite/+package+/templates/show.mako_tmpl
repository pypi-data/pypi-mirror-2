# -*- coding: utf-8 -*-
<%inherit file="/core/hide-right.mako"/>
<%def name="header()">
    % if c.title is not UNDEFINED:
    <title>${c.title or 'Index'}</title>
    % else:
    <title>{{package}} :: Entity</title>
    % endif
</%def>
<%def name="content()">
    <h1>${c.name}</h1>
    <div>${c.content|n}</div>
</%def>
<%def name="lhcolumn()">
    <%include file="lhcolumn.mako" />
</%def>
