# -*- coding: utf-8 -*-
## Based on standard 2-col Hide-Right derivation
## Defines "content" and "lhcolumn" fragments

<%inherit file="/core/hide-both.mako"/>
<%def name="header()">
    <link type="text/css" rel="stylesheet" href="/css/sphinx.css" media="screen" />
    % if c.title is not UNDEFINED:
    <title>${c.title|n}</title>
    % else:
    <title>{{package}} :: Default</title>
    % endif
</%def>
<%def name="content()">
        ${c.page['body']|n}
</%def>
