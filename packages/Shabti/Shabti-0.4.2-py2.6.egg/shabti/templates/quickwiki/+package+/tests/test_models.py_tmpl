from {{package}}.tests import *
from {{package}}.tests import Session, Page, TestModel

class TestPages(TestModel):
    
    def test_index(self):
        q = Session.query(Page).all()
        print(q)
        assert q == []
        page = Page()
        page.title = u'TestPage'
        page.content = u'Welcome to the QuickWiki test page.'
        Session.add(page)
        Session.commit()
        q = Session.query(Page).all()
        print(q)
        assert q == [page]
    

