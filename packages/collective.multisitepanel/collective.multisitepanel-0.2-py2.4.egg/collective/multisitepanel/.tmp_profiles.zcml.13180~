<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    i18n_domain="rer.dashboard">

  <genericsetup:registerProfile
      name="default"
      title="RER Dashboard"
      directory="profiles/default"
      description='Extension profile for the "RER Dashboard" Plone theme.'
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <genericsetup:registerProfile
      name="uninstall"
      title="RER Dashboard uninstall"
      directory="profiles/uninstall"
      description='Uninstall profile for the "RER Dashboard" Plone theme.'
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <genericsetup:importStep
      name="rer.dashboard.various"
      title="RER Dashboard: miscellaneous import steps"
      description="Various import steps that are not handled by GS import/export handlers."
      handler="rer.dashboard.setuphandlers.setupVarious">
    <depends name="skins"/>
  </genericsetup:importStep>

</configure>
