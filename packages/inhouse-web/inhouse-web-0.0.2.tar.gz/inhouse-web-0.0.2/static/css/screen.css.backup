html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{border:0;outline:0;font-size:100%;vertical-align:baseline;background:transparent;margin:0;padding:0}
body{line-height:1}
ol,ul{list-style:none}
blockquote,q{quotes:none}
blockquote:before,blockquote:after,q:before,q:after{content:none}
:focus{outline:0}
ins{text-decoration:none}
del{text-decoration:line-through}
table{border-collapse:collapse;border-spacing:0}

@font-face {
  font-family: Helvetica;
  src: url('/static/fonts/Helvetica.ttf');
}

@font-face {
  font-family: Droid-Sans;
  src: url('/static/fonts/DroidSans.ttf');
}

@font-face {
  font-family: Droid-Sans;
  font-weight: bold;
  src: url('../DroidSans-Bold.ttf');
}

header, footer, aside, nav, article {
  display: block;
}

header {
  background-color: white;
  text-align: right;
}

body {
  color: #333;
  font-family: arial, sans-serif;
  font-size: 83%;
  margin-left: 10px;
  margin-right: 10px;
  padding: 5px;
  background-color: white;
}

nav {
  clear: both;
  position:relative;
  height: 26px;
}

nav ul {
  float:left;
  list-style-image:none;
  list-style-position:outside;
  list-style-type:none;
  margin: 0 0 0 5px;
  padding: 0;
}

nav ul li {
  background-color: #dddddd;
  -moz-border-radius-topleft: 5px;
  -moz-border-radius-topright: 5px;
  -webkit-border-top-left-radius: 5px;
  -webkit-border-top-right-radius: 5px;
  float: left;
  margin: 0 4px 0 0;
  padding: 0;
}

nav ul li a {
  background-position: right top;
  background-repeat: no-repeat;
  display: block;
  font-weight: bold;
  height: 26px;
  line-height: 27px;
  padding: 0 8px;
  vertical-align: middle;
  color: #333;
}

nav ul li a:hover {
  background-color: #6c98c4;
  -moz-border-radius-topleft: 5px;
  -moz-border-radius-topright: 5px;
  -webkit-border-top-left-radius: 5px;
  -webkit-border-top-right-radius: 5px;
  color: white;
  text-decoration: none;
}

#sub-navigation {
  /*background-color: #ddeeff;*/
  background-color: #E5ECF9;
  /*border-top: 4px solid #6c98c4;*/
  border-top: 2px solid #36C;
  /*padding: 6px;*/
  padding: 5px 5px 5px 5px;
  text-align: left;
  min-height: 16px;
  height: 22px;
}

#sub-navigation ul {
  list-style-image:none;
  list-style-position:outside;
  list-style-type:none;
}

#sub-navigation ul li {
  background-color: #E5ECF9;
  float:left;
  margin: 0;
  padding: 0;
}

#sub-navigation ul li a {
  color: #4272DB;
  display: inline;
  font-weight: normal;
  height: 22px;
  line-height: 23px;
}

#sub-navigation ul li a:hover {
  background-color: #E5ECF9;
  color: #4272DB;
  font-weight: normal;
  text-decoration: underline;
}

#intro {
  margin-bottom: 0.5em;
}

#content {
  display: table;
}

aside {
  display: table-cell;
  border-right: 2px solid #36C;
  padding-left: .8em;
  padding-right: .8em;
  vertical-align: top;
  width: 350px;
  min-height: 480px;
}

#main-content {
  display: table-cell;
  padding-left: .8em;
  padding-right: .8em;
  width: 80%;
}

#about, #shortcuts {
  margin-top: 10px;
  text-align: center;
}

#about {
  font-size: 0.88em;
}

h1, h2, h3, h4, h5, h6 {
  padding-bottom:0;
  margin-bottom: 5px;
  letter-spacing: -0,018em;
}

h1 {
  color: black;
  font-weight: bold;
  font-size: 1.6em;
  margin-top: 8px;
  text-transform: normal;
}

h2 {
  color: black;
  display: block;
  font-weight: bold;
  font-size: 1.375em;
}

h3 {
  color: #4272db;
  font-weight: bold;
  font-size: 1.25em;
  padding: 8px;
}

em {
  font-style:normal;
  font-weight:bold;
}

a {
  color: #4272db;
  text-decoration:none;
}

a:focus, a:hover {
  text-decoration:underline;
}

a:visited {
}

label {
  float: right;
  margin-right: 15px;
}

input  {
  padding-left:1px;
  padding-right:1px;
}

img {
  float: left;
  display:block;
}

pre, code {
  font-family: monospace;
  font-size: 12px;
  margin: 15px;
}

th {
  padding-top: 3px;
  vertical-align: top;
}

div {
  display: block;
}

.sprite {
  background: url('/static/images/icons.png') no-repeat top left;
  display: block;
  float: left;
  margin-right: 6px;
  margin-top: 2px;
}

.closedtriangle { background-position: 0 0; width: 12px; height: 9px; }
.left_big { background-position: 0 -59px; width: 29px; height: 17px; }
.minus { background-position: 0 -126px; width: 12px; height: 12px; }
.pause { background-position: 0 -188px; width: 12px; height: 12px; }
.plus { background-position: 0 -250px; width: 12px; height: 12px; }
.right_big { background-position: 0 -312px; width: 29px; height: 17px; }
.star { background-position: 0 -379px; width: 14px; height: 14px; }
.star_grey { background-position: 0 -443px; width: 14px; height: 14px; }
.start { background-position: 0 -507px; width: 12px; height: 12px; }




#logo {
  display: block;
  float: left;
  font-size: 180%;
  font-weight: bold;
  margin: 20px 25px 20px 10px;
  margin-top: 20px;
  margin-top: 20px;
}

.popup {
  visibility: hidden;
  background: black;
  color: white;
  padding: 1em;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  opacity: 0.85;
  -moz-opacity: 0.85;
  -khtml-opacity: 0.85;
  filter: alpha(opacity=85);
  position: absolute;
  width: 30%;
  z-index: 1;
  overflow-x: hidden;
  overflow-y: auto;
}

.pagination {
  padding: 2px;
  margin-top: 3px;
  margin-bottom 3px;
  text-align:right;
}

.eg {
  color: #666;
  font-size: 90%;
}

.default-list {
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
/*  background-color: #ebeff9;*/
  background-color: #E5ECF9;
  border: 2px solid #d2e6ff;
  margin-bottom: 10px;
  padding: 5px;
}

.default-list table {
  width: 100%;
}

.default-list table .first {
  /*border-left: 1px solid #bad9ff;*/
}

.default table .last {
  /*border-right: 1px solid #bad9ff;*/
}

.default-list table th {
  background-color: #D4E6FC;
  border-bottom: 1px solid #c2cff1;
  padding-left: 3px;
  padding-right: 5px;
  padding-top: 8px;
  padding-bottom: 5px;
  font-weight: bold;
  text-align: left;
}

.default-list table tr {
  border-bottom: thin solid #e8eef7;
  background-color: #FBFBFB;
}

.default-list table .selected {
  background-color: #eaf1ff !important;
}

.default-list table td {
  background-color: #ffffff;
  padding: 3px;
}

.default-list .header {
  background-color: #e7efff;
  padding: 6px;
}

.default-list tr.alt td {
	background: #f4f8ff;
}

.default-list tr.over td {
	background: #fffdd2;
}

/* Message box */

.wrapper, .message-box {
  /*height: 500px;*/
  height: 50px;
  width: 600px;
  z-index: 1;
}
.wrapper {
  /*bottom: 50%;*/
  top: 10em;
  right: 50%;
  position: absolute;
}
.message-box {
  font-weight: bold;
  left: 50%;
  position: relative;
  /*top: 50%;*/
  text-align: center;
}

.message-box ul {
  background-color: #FFF1A8;
  padding: 0.6em;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
}

.message-box a {
  text-decoration: underline;
}

/* Module boxes */

.module {
  margin-bottom: 5px;
}

.module .header {
  background: #DDD;
  color: #333;
  cursor: pointer;
  margin-bottom: 0;
}

.module .header#green {
  background-color: #a5c1a5;;
}

.module .header a {
  color: #333;
}

.module .header a:hover {
  color: #4272db;
  text-decoration: none;
}

.module .body {
  background: #EEE;
  display: block;
  overflow: hidden;
  /*padding: 4px 8px;*/
  padding: 10px 15px 10px 15px;
}

.module .body#green {
  background-color: #D2E6D2;
}

/* Cards */

.card {
  border-color: #BBB;
  border-bottom-left-radius: 5px 5px;
  border-bottom-right-radius: 5px 5px;
  border-style: solid;
  border-width: 1px;
  border-top-left-radius: 5px 5px;
  border-top-right-radius: 5px 5px;
  overflow: hidden;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -webkit-box-shadow: #E3E5EB 0px 1px 1px;
}

.card-common {
  background-color: white;
  margin: 0px 10px;
  zoom: 1;
}

.card-bottom {
  background-color: #F3F5FC;
  /*color: #24B;*/
  color: #666;
  direction: ltr;
  font-size: 90%;
  padding: 4px 13px 7px;
}

.entry {
  position: relative;
  zoom: 1;
  padding: 5px 0px;
}

.entry .entry-title {
  padding: 0;
}

.entry .entry-content {
  padding-bottom: 1em;
  padding-top: 1em;
}

.entry .entry-main {
  margin-left: 30px;
  margin-right: 10px;
  zoom: 1;
}

.entry .entry-body {
  padding-top: 1em;
  /*ax-width: 650px;*/
}

.entry .entry-icons {
  left: 20px;
  top: 20px;
  position: absolute;
    /*
left: 2px;
position: absolute;
top: 2px;
width: 18px;
*/
  vertical-align: top;
}

.entry .entry-date {
  color: #666;
  font-size: 90%;
  text-decoration: none;
}

.entry .entry-date {
  float: right;
}

/* Timeline */

.timeline-list {
  background-color: #D4E6FC;
}

.timeline-list h4 {
border-top: 1px solid #BBB;
font-size: 100%;
font-weight: normal;
line-height: 160%;
margin: 0px;
padding: 0px 0px 0px 1em;
padding-left: 1em;
padding-left: 1em;
}

ul.timeline li {
  background-color: white;
  border-top: 1px solid #BBB;
  border-left: 1px solid #C3D9FF;
  border-right: 1px solid #C3D9FF;
  clear: both;
  margin: 0px;
  /*padding: 0,375em 0px;*/
  padding: 0.5em 0px 0.5em 0.8em;
  z-index: 0;
}

ul.timeline span.expand {
  padding-right: 2em;
}

ul.timeline span.date {
background-position: 5px 50%;
background-repeat: no-repeat;
color: #5F5F5F;
float: left;
padding-right: 1em;
text-align: right;
width: 7,5em;
}

ul.timeline span.details {
}

ul.timeline div.details {
  color: #5F5F5F;
  border-left: 0.3em solid #C3D9FF;
  margin: 0.8em 0 0.8em 8.1em;
  padding: 0.5em;
}

/* Calendar */

.calendar {
/*
  background-color: #eaf1ff;
  -moz-border-radius-bottomleft: 5px;
  -moz-border-radius-bottomright: 5px;
  -moz-border-radius-topleft: 5px;
  -moz-border-radius-topright: 5px;
  border-color: #bdd4ff;
  border-style: solid;
  border-width: 1px 1px 1px;
  padding: 5px;
  width: 96%;
  */
    -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
/*  background-color: #ebeff9;*/
  background-color: #E5ECF9;
  border: 2px solid #d2e6ff;
  margin-bottom: 10px;
  padding: 5px;
}

.calendar .pagination {
  background-color: #e7efff;
  padding:3px;
  text-align:right;
}

.calendar table th, .calendar table td {
  padding: 0.5em;
}

.calendar table th {
  font-weight: bold;
  /*background-color: lightgray;*/
  border-right: 1px solid lightgray;
  border-top: 1px solid lightgray;
  padding: 5px 8px 5px 8px;
}

.calendar table th.header {
  background-color: #e7efff;
  text-align: center;
}

.calendar table tr {
  border-bottom: thin solid lightgray;
  background-color: #EEEEEC;
}

.calendar table td {
  background-color: #FFFFFF;
  text-align: center;
}

.calendar table td.invalid {
  color: #CCCCCC;
}

.calendar table td.current {
  /*background-color: #b9ccf0;*/
  background: #FFF7D7;
  /*border: 1px solid #FAD163;*/
}

.calendar table td.selected {
  background-color: #e3edff;
}