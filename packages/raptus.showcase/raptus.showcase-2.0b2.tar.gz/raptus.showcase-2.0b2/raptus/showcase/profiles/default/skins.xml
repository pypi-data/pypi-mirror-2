<?xml version="1.0"?>
<object name="portal_skins">
    
  <object name="showcase" meta_type="Filesystem Directory View"
    directory="raptus.showcase:skins/showcase"/>
  <object name="showcaseboxwidget" meta_type="Filesystem Directory View"
    directory="raptus.showcase:skins/showcaseboxwidget"/>
    
  <skin-path name="*">
    <layer name="showcase" insert-after="custom" />
  </skin-path>
  <skin-path name="*">
    <layer name="showcaseboxwidget" insert-after="custom" />
  </skin-path>
</object>
