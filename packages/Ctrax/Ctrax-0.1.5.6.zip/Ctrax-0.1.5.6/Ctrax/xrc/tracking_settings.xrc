                          <object class="sizeritem">
                            <object class="wxStaticText" name="automatic_shape_text">
                              <label>Bounds on shape not up to date. </label>
                              <font>
                                <size>9</size>
                                <family>swiss</family>
                                <style>italic</style>
                                <weight>normal</weight>
                                <underlined>0</underlined>
                                <face>Sans</face>
                              </font>
                            </object>
                            <flag>wxALIGN_CENTRE_HORIZONTAL</flag>
                          </object>
                        </object>
                        <size>360,160</size>
                        <style>wxTAB_TRAVERSAL</style>
                        <enabled>1</enabled>
                      </object>
                      <flag>wxALIGN_CENTRE_VERTICAL|wxALIGN_CENTRE_HORIZONTAL</flag>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,20</size>
                        <style>wxLI_HORIZONTAL</style>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxRadioButton" name="manual_bounds">
                        <label>Manually Set Bounds on Shape</label>
                        <font>
                          <size>10</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>bold</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                      <border>10</border>
                    </object>
                    <object class="spacer">
                      <size>0,10</size>
                    </object>
                    <object class="sizeritem">
                      <object class="wxPanel" name="manual_panel">
                        <object class="wxBoxSizer">
                          <orient>wxVERTICAL</orient>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <size>80,20</size>
                                  <label>Minimum</label>
                                  <style>wxALIGN_CENTRE</style>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <size>80,20</size>
                                  <label>Mean</label>
                                  <style>wxALIGN_CENTRE</style>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <size>80,20</size>
                                  <label>Maximum</label>
                                  <style>wxALIGN_CENTRE</style>
                                </object>
                              </object>
                              <orient>wxHORIZONTAL</orient>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <orient>wxHORIZONTAL</orient>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="min_area">
                                  <tooltip>Lower bound on fly area (pixels-squared)</tooltip>
                                  <help>Lower bound on fly area (pixels-squared)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="mean_area">
                                  <tooltip>Mean fly area (pixels-squared)</tooltip>
                                  <help>Mean fly area (pixels-squared)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="max_area">
                                  <tooltip>Upper bound on fly area (pixels-squared)</tooltip>
                                  <help>Upper bound on fly area (pixels-squared)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <label>  Area of Target</label>
                                </object>
                              </object>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <orient>wxHORIZONTAL</orient>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="min_major">
                                  <tooltip>Lower bound on major axis length (pixels)</tooltip>
                                  <help>Lower bound on major axis length (pixels)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="mean_major">
                                  <tooltip>Mean major axis length</tooltip>
                                  <help>Mean major axis length</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="max_major">
                                  <tooltip>Upper bound on major axis length</tooltip>
                                  <help>Upper bound on major axis length</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <label>  Major axis length</label>
                                </object>
                              </object>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <orient>wxHORIZONTAL</orient>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="min_minor">
                                  <tooltip>Lower bound on minor axis length (pixels)</tooltip>
                                  <help>Lower bound on minor axis length (pixels)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="mean_minor">
                                  <tooltip>Mean minor axis length (pixels)</tooltip>
                                  <help>Mean minor axis length (pixels)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="max_minor">
                                  <tooltip>Upper bound on minor axis length (pixels)</tooltip>
                                  <help>Upper bound on minor axis length (pixels)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <label>  Minor axis length</label>
                                </object>
                              </object>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <orient>wxHORIZONTAL</orient>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="min_ecc">
                                  <tooltip>Lower bound on eccentricity (minor / major axis length)</tooltip>
                                  <help>Lower bound on eccentricity (minor / major axis length)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="mean_ecc">
                                  <tooltip>Mean eccentricity (minor / major axis length)</tooltip>
                                  <help>Mean eccentricity (minor / major axis length)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="max_ecc">
                                  <tooltip>Upper bound on eccentricity (minor / major axis length)</tooltip>
                                  <help>Upper bound on eccentricity (minor / major axis length)</help>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <label>  Eccentricity</label>
                                </object>
                              </object>
                            </object>
                          </object>
                        </object>
                        <size>360,160</size>
                        <style>wxTAB_TRAVERSAL</style>
                        <enabled>1</enabled>
                      </object>
                      <flag>wxALIGN_CENTRE|wxALIGN_CENTRE_VERTICAL|wxALIGN_CENTRE_HORIZONTAL</flag>
                    </object>
                  </object>
                </object>
              </object>
              <object class="notebookpage">
                <label>Motion</label>
                <object class="wxPanel" name="motion_model_panel">
                  <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Motion Model</label>
                        <font>
                          <size>10</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>bold</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="spacer">
                      <size>0,10</size>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Angle Weight: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="angle_weight">
                            <value>0</value>
                          </object>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Weight of the angle parameter in computing distance between predicted target and observation positions. Angle distance multiplied by weight is added to unweighted Euclidean distance between centers. </label>
                        <size>400,52</size>
                        <font>
                          <size>8</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,5</size>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Max Jump Distance: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="max_jump"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Maximum distance between predicted position and observation for a target identity and observation to be matched. </label>
                        <size>400,28</size>
                        <font>
                          <size>8</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,5</size>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Center Dampening: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="center_dampen"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Angle Dampening: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="angle_dampen"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Predicted position is previous position plus (1 - dampen) times previous velocity. If dampen is 1, then predicted position is just previous position. If dampen is 0, then predicted position is constant velocity prediction. </label>
                        <size>400,50</size>
                        <font>
                          <size>8</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
              <size>400,420</size>
              <style>wxNB_DEFAULT</style>
              <object class="notebookpage">
                <label>Observation</label>
                <object class="wxPanel" name="observation_panel">
                  <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>Observation Parameters</label>
                        <font>
                          <size>10</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>bold</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Max Area Delete: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="max_area_delete"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <size>400,50</size>
                        <label>If a detected connected component cannot be merged with other connected components and has area &lt;= Max Area Delete, then this connected component is ignored. Area is measured in squared pixels. </label>
                        <font>
                          <size>8</size>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <family>swiss</family>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,5</size>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <size>300,20</size>
                            <label>Min Area Ignore: </label>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="min_area_ignore"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <size>400,40</size>
                        <label>Extremely large connected components may be caused by objects other than flies or lighting changes. Enter the minimum area of large foreground detections to be ignored.</label>
                        <font>
                          <size>8</size>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <family>swiss</family>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,5</size>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Max Penalty Merge: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="max_penalty_merge"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>We check to see if small connected components can be merged by flipping the labels of some pixels from background to foreground. If the total difference of these background pixels from the foreground threshold is less than Max Penalty Merge, then we perform the merge. </label>
                        <size>400,64</size>
                        <font>
                          <size>8</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,5</size>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <label>Lower Threshold: </label>
                            <size>300,20</size>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="lower_thresh"/>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <size>400,50</size>
                        <label>If the area of a connected component is small, we will try to lower the foreground/background threshold to include more pixels in the connected component. We lower the threshold to Lower Thresh times the current threshold. </label>
                        <font>
                          <size>8</size>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <family>swiss</family>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticLine">
                        <size>400,5</size>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxBoxSizer">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                          <object class="wxStaticText">
                            <size>300,20</size>
                            <label>Max Clusters per Blob: </label>
                          </object>
                        </object>
                        <object class="sizeritem">
                          <object class="wxTextCtrl" name="maxclustersperblob">
                            <value>10</value>
                          </object>
                        </object>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <size>400,50</size>
                        <label>Maximum number of clusters Ctrax will try to split a connected component into during the forward pass.</label>
                        <font>
                          <size>8</size>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <family>swiss</family>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
              <object class="notebookpage">
                <label>Hindsight</label>
                <object class="wxPanel" name="hindsight_panel">
                  <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                    <object class="sizeritem">
                      <object class="wxCheckBox" name="do_fix_split">
                        <label>Fix Split Detections</label>
                        <checked>1</checked>
                        <font>
                          <size>10</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>bold</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxStaticText">
                        <label>If checked, Ctrax will look for sequences in which a single target is mistakenly split into two observations. These observations will be merged. </label>
                        <size>400,38</size>
                        <font>
                          <size>8</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>normal</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxPanel" name="splitdetection_panel">
                        <object class="wxBoxSizer">
                          <orient>wxVERTICAL</orient>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <orient>wxHORIZONTAL</orient>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <label>Max Sequence Length: </label>
                                  <size>300,20</size>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="splitdetection_length"/>
                              </object>
                            </object>
                          </object>
                          <object class="sizeritem">
                            <object class="wxBoxSizer">
                              <orient>wxHORIZONTAL</orient>
                              <object class="sizeritem">
                                <object class="wxStaticText">
                                  <label>Max Penalty Merge: </label>
                                  <size>300,20</size>
                                </object>
                              </object>
                              <object class="sizeritem">
                                <object class="wxTextCtrl" name="splitdetection_distance"/>
                              </object>
                            </object>
                          </object>
                        </object>
                        <enabled>1</enabled>
                      </object>
                    </object>
                    <object class="sizeritem">
                      <object class="wxCheckBox" name="do_fix_merged">
                        <label>Fix Merged Detections</label>
                        <checked>1</checked>
                        <font>
                          <size>10</size>
                          <family>swiss</family>
                          <style>normal</style>
                          <weight>bold</weight>
                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="sizeritem">
