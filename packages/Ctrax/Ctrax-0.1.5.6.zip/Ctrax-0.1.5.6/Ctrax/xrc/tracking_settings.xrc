                          <underlined>0</underlined>
                          <face>Sans</face>
                        </font>
                      </object>
                    </object>
                    <object class="spacer">
                      <size>0,10</size>
                    </object>
                    <object class="sizeritem">
                      <object class="wxPanel" name="automatic_panel">
                        <object class="wxBoxSizer">
                          <orient>wxVERTICAL</orient>
