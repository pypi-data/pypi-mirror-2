<?xml version="1.0" ?>
<style-scheme id="blueprint" name="Blueprint" version="1.0">
	<author/>
	<_description>
		Blueprint theme
	</_description>
	<style background="#0e4072" foreground="#cccccc" name="text"/>
	<style background="#90fff0" foreground="#2050d0" name="search-match"/>
	<style foreground="#808080" name="line-numbers"/>
	<style background="#darkblue" name="diff:added-line"/>
	<style background="#darkmagenta" name="diff:changed-line"/>
	<style background="#DarkCyan" bold="true" foreground="#Blue" name="diff:removed-line"/>
	<style background="#ffaf38" foreground="#000020" name="cursor"/>
	<style foreground="#80a0ff" name="def:comment"/>
	<style foreground="#ffa0a0" name="def:constant"/>
	<style foreground="#Orange" name="def:specials"/>
	<style foreground="#40ffff" name="def:identifier"/>
	<style bold="true" foreground="#white" name="def:statement"/>
	<style foreground="#999999" name="def:preprocessor"/>
	<style foreground="#60ff60" name="def:type"/>
</style-scheme>

