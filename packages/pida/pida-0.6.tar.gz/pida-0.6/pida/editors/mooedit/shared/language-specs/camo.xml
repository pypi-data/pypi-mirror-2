<?xml version="1.0" ?>
<style-scheme id="camo" name="Camo" version="1.0">
	<author/>
	<_description>
		Camo theme
	</_description>
	<style background="#grey15" foreground="#bisque" name="text"/>
	<style background="#bisque3" foreground="#snow" name="cursor"/>
	<style background="#grey15" foreground="#DarkOliveGreen1" name="diff:added-line"/>
	<style background="#grey15" foreground="#PaleGreen" name="diff:changed-line"/>
	<style background="#grey15" foreground="#red" name="diff:removed-line"/>
	<style background="#grey15" foreground="#OliveDrab4" name="line-numbers"/>
	<style background="#grey15" foreground="#DarkSalmon" name="search-match"/>
	<style foreground="#tan" name="def:comment"/>
	<style foreground="#khaki" name="def:constant"/>
	<style foreground="#moccasin" name="def:string"/>
	<style foreground="#chocolate" name="def:number"/>
	<style foreground="#OliveDrab3" name="def:boolean"/>
	<style foreground="#khaki4" name="def:identifier"/>
	<style foreground="#OliveDrab4" name="def:function"/>
	<style foreground="#khaki" name="def:statement"/>
	<style foreground="#DarkKhaki" name="def:keyword"/>
	<style foreground="#khaki4" name="def:preprocessor"/>
	<style foreground="#khaki3" name="def:type"/>
	<style foreground="#IndianRed" name="def:specials"/>
	<style background="#red" foreground="#bisque" name="def:error"/>
</style-scheme>

