{
  "name": "Django Desktop Notifications",
  "version": "1.2",
  "description":
    "Display some informations from django application directly on the end user desktop.",
  "icons": {"64": "64.png", "128": "128.png"},
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "options_page": "options.html",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "64.png",
    "default_title": "Django notifications",
    "default_popup": "popup.html"
  }
}
