# IMPORTANT - THE STAGES ARE ASSUMED TO BE IN TIME-ORDER.
                 
set Stages := Ano1Stage Ano2Stage;

set Nodes := RootNode 
             Leaf1
             Leaf2
             Leaf3
             Leaf4 ;

param NodeStage := RootNode      		Ano1Stage 
	   	 Leaf1 			Ano2Stage
	   	 Leaf2 			Ano2Stage
	   	 Leaf3 			Ano2Stage
	   	 Leaf4 			Ano2Stage ;

set Children[RootNode] :=              Leaf1
             Leaf2
             Leaf3
             Leaf4 ;

param ConditionalProbability := RootNode          	1.0
				        Leaf1 			0.25
				        Leaf2 			0.25
				        Leaf3 			0.25
				        Leaf4 			0.25 ;

set Scenarios := ForestDavis4 
                 ForestDavis8 
                 ForestDavis12
                 ForestDavis16 ;

param ScenarioLeafNode := ForestDavis4	Leaf1
				  ForestDavis8	Leaf2
				  ForestDavis12	Leaf3
				  ForestDavis16	Leaf4 ;

set StageVariables[Ano1Stage] := delta[*,Ano1]
                                 gamma[*,Ano1]
                                 f[*,Ano1]
                                 z[*,Ano1] ;

set StageVariables[Ano2Stage] := delta[*,Ano2]
                                 gamma[*,Ano2]
                                 f[*,Ano2]
                                 z[*,Ano2] ;

param StageCostVariable := Ano1Stage AnoProfit[Ano1]
                           Ano2Stage AnoProfit[Ano2] ;


