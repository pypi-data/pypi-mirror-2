<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="text/x-kfcom">
    <comment>Saved Kupfer Command</comment>
    <comment xml:lang="cs">Uložený příkaz aplikace Kupfer</comment>
    <comment xml:lang="de">Gespeicherter Kupfer-Befehl</comment>
    <comment xml:lang="es">Comando de Kupfer guardado</comment>
    <comment xml:lang="fr">Commande Kupfer sauvegardée</comment>
    <comment xml:lang="it">Comando salvato</comment>
    <comment xml:lang="pl">Zapisane polecenie programu Kupfer</comment>
    <comment xml:lang="sl">Shranjen ukaz Kupfer</comment>
    <comment xml:lang="sv">Sparat Kupfer-kommando</comment>
    <glob pattern="*.kfcom"/>
    <magic priority="50">
      <match value="#!/usr/bin/env kupfer-exec" type="string" offset="0"/>
    </magic>
    <generic-icon name="text-x-script"/>
  </mime-type>
</mime-info>