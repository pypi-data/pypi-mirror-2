import gtk
import logging

import lsrsidmodules

if __name__ == "__main__":
    logging.basicConfig(level=logging.DEBUG)
    gtk.gdk.threads_init()
    main = lsrsidmodules.Main()
    if not main.cancel:
        gtk.main()
