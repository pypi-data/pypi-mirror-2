<tal:block xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="bda.contentproxy">

<metal:block use-macro="here/global_defines/macros/defines" />
  
  <tal:block
    define="templ_id plone_view/getViewTemplateId;
            view_template python: context.restrictedTraverse(templ_id);
            z3view python:path('nocall:context/@@'+templ_id+'|nothing');
            view python:z3view or view">

    <metal:macro use-macro="view_template/macros/main" />

  </tal:block>

</tal:block>