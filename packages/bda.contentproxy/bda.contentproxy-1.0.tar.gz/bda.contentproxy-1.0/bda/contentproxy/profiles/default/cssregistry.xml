<?xml version="1.0"?>
<!-- This file holds the setup configuration for the portal_css tool. -->

<object name="portal_css">
  <stylesheet
    title="++resource++bda.contentproxy.contentproxy.css"
    cacheable="True"
    compression="safe"
    cookable="True"
    enabled="on"
    expression="python:object.portal_type == 'ATProxiedContent' or object.restrictedTraverse('@@plone').getCurrentUrl().find('/@@proxy') != -1"
    id="++resource++bda.contentproxy.contentproxy.css"
    media=""
    rel="stylesheet"
    rendering="import"/>
</object>
