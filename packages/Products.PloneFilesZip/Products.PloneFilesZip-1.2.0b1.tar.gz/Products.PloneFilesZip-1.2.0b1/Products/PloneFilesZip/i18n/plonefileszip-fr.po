# Visitors UI

msgid "title_download_zip"
msgstr "Archive ZIP des fichiers"

msgid "explain_download_zip"
msgstr "Télécharger les fichiers de ${count} contenu(s) dans l'archive ZIP ${filename}"

msgid "Download"
msgstr "Télécharger"

# Configlet

msgid "legend_files_zip_prefs"
msgstr "Paramètres PloneFilesZip"

msgid "legend_files_zip_prefs"
msgstr "Vos préférences PloneFilesZip"

msgid "label_select_portal_types"
msgstr "Types de contenus (portal types) inclus"

msgid "help_select_portal_types"
msgstr "Sélectionnez les types de contenus dont les champs fichier (FileField) ou compatibles fourniront des fichiers dans les archives ZIP."

msgid "label_include_rdf"
msgstr "Inclure les fichiers RDF ?"

msgid "help_include_rdf"
msgstr "Voulez-vous inclure les métadonnées Dublin Core sous forme de fichiers RDF dans les archives ZIP ?"

msgid "label_zip_charset"
msgstr "Encodage des noms de fichiers"

msgid "help_zip_charset"
msgstr "Veuillez sélectionner un encodage valide des noms de fichiers dans les archives ZIP. "
       "L'encodage 'cp1252' convient la plupart du temps mais vous pouvez en choisir un autre."

# Policies

msgid "structure_policy_label"
msgstr "Structure interne de l'archive zip"

msgid "structure_policy_help"
msgstr "Déterminez comment l'arborescence interne du zip sera construite"


msgid "deep_inner_zip_structure"
msgstr "Deep zip inner structure policy"

msgid "deep_structure_policy_help"
msgstr "La structure interne de l'archive correspond exactement à la structure interne des objets : si le document a des sous-attributs, ils correspondront à des sous-répertoires. Il ne peut y avoir de noms dupliqués."

msgid "simple_inner_zip_structure"
msgstr "Simple zip inner structure policy"

msgid "simple_structure_policy_help"
msgstr "La structure interne de l'archive correspond à la structure telle qu'elle est conçue par le visiteur. Les fichiers sont enregistrés dans un répertoire du nom de leur document."

msgid "fs_like_inner_zip_structure"
msgstr "File system like zip inner structure policy"

msgid "fs_like_structure_policy_help"
msgstr "Les fichiers sont stockés dans un répertoire du nom de leur folder."

msgid "flat_inner_zip_structure"
msgstr "Flat zip inner structure policy"

msgid "flat_structure_policy_help"
msgstr "Tous les fichiers sont enregistrés à la racine de l'archive."










