        title="Upgrade uwosh.northstar to 0.3"
        description="uwosh.northstar upgrade step"
        source="*"
        destination="0.3"
        handler=".upgrades.upgrade_to_0_3"
        profile="uwosh.northstar:default" />
        
    <gs:upgradeStep
        title="Upgrade uwosh.northstar to 0.5b1"
