        profile="uwosh.northstar:default" />
        
    <gs:upgradeStep
        title="Upgrade uwosh.northstar to 0.5b2"
        description="uwosh.northstar upgrade step"
        source="0.5b1"
        destination="0.5b2"
        handler=".upgrades.upgrade_to_0_5b2"
        profile="uwosh.northstar:default" />
               
</configure>
