<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      i18n:domain="collective.imagetags"
      tal:omit-tag="">

    <script type="text/javascript" tal:content="structure view/js_vars">
        ImageTagsLabels = {
            tagFormTitle: 'Tag details',
            removeLinkText: 'Remove',
            removeConfirmText: 'Are you sure you want to remove this tag?',
            editLinkText: 'Edit',
            yesButtonText: 'Yes',
            noButtonText: 'No',
            saveButtonText: 'Save',
            hyphenLabel: ' - '
        };
    </script>

    <tal:defines define="view_id view/getId">
        <div id="" tal:attributes="id view_id">
            <span tal:replace="structure view/contents" />
        </div>
    </tal:defines>
    
    
    
</html>

