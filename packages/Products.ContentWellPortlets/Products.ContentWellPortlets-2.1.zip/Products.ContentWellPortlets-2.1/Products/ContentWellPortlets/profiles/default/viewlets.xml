<?xml version="1.0"?>
<!-- Reorder the viewlets above the content well because by default the new viewlet is plonked underneath the existing viewlets -->

<object>
	<order manager="plone.portalfooter" skinname="*">
  		<viewlet name="contentwellportlets.footerportlets" insert-before = "plone.footer"/> 
  		<viewlet name="plone.footer" />
		<viewlet name="plone.colophon"/>
 </order>

</object>