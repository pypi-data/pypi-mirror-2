<configure
    xmlns="http://namespaces.zope.org/zope"
    i18n_domain="collective.classification">

    <utility
        provides="..interfaces.ITokenizer"
        component=".languagespecific.english_tokenizer"
        name="en"
    />

    <utility
        provides="..interfaces.ITokenizer"
        component=".languagespecific.dutch_tokenizer"
        name="nl"
    />

    <utility
        provides="..interfaces.IPOSTagger"
        component=".languagespecific.english_tagger"
        name="en"
    />

    <utility
        provides="..interfaces.IPOSTagger"
        component=".languagespecific.dutch_tagger"
        name="nl"
    />

</configure>
