<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="kuliaden Theme">

 <object name="medialog_kuliadentheme_custom_images"
    meta_type="Filesystem Directory View"
    directory="medialog.kuliadentheme:skins/medialog_kuliadentheme_custom_images"/>
 <object name="medialog_kuliadentheme_custom_templates"
    meta_type="Filesystem Directory View"
    directory="medialog.kuliadentheme:skins/medialog_kuliadentheme_custom_templates"/>
 <object name="medialog_kuliadentheme_styles"
    meta_type="Filesystem Directory View"
    directory="medialog.kuliadentheme:skins/medialog_kuliadentheme_styles"/>

 <skin-path name="kuliaden Theme" based-on="Plone Default">
  <layer name="medialog_kuliadentheme_custom_images"
     insert-after="custom"/>
  <layer name="medialog_kuliadentheme_custom_templates"
     insert-after="medialog_kuliadentheme_custom_images"/>
  <layer name="medialog_kuliadentheme_styles"
     insert-after="medialog_kuliadentheme_custom_templates"/>
 </skin-path>

</object>
