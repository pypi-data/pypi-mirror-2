<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool">
  <object name="Quaestrio_styles" meta_type="Filesystem Directory View"
    directory="Quaestrio/skins/Quaestrio_styles"/>
  <object name="Quaestrio_images" meta_type="Filesystem Directory View"
    directory="Quaestrio/skins/Quaestrio_images"/>
  <object name="Quaestrio_templates" meta_type="Filesystem Directory View"
    directory="Quaestrio/skins/Quaestrio_templates"/>
  <object name="Quaestrio_scripts" meta_type="Filesystem Directory View"
    directory="Quaestrio/skins/Quaestrio_scripts"/>
  <skin-path name="*">
    <layer name="Quaestrio_images" insert-after="custom" />
    <layer name="Quaestrio_scripts" insert-after="custom" />
    <layer name="Quaestrio_styles" insert-after="custom" />
    <layer name="Quaestrio_templates" insert-after="custom" />
  </skin-path>
</object>
