<html xmlns="http://www.w3.org/1999/xhtml" xmlns:py="http://genshi.edgewall.org/"
    xmlns:xi="http://www.w3.org/2001/XInclude" style="height:100%;">
    <head py:if="section is None"></head>
    <body>
        <py:for each="title in ['One', 'Two', 'Three']">
            <div py:if="section is None or section == title.lower()">
                <h2>Section ${title}</h2>
                hello world ${title.lower()}
            </div>
        </py:for>
    </body>
</html>
