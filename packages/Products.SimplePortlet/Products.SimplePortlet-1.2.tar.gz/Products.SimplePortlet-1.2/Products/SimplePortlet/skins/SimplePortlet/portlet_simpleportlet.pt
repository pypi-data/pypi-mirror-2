<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      i18n:domain="plone">
      
<body>

    <div metal:define-macro="portlet_left"
         tal:omit-tag="" tal:define="portlets python:here.portlet_manager.getPortlets(here, 'columnOne')">

      <tal:portlets tal:repeat="portlet portlets">
        <tal:block tal:define="macro_path portlet/getMacro;
                               macro python:path(macro_path)">
          <metal:macro use-macro="macro" />
        </tal:block>
      </tal:portlets>
    </div>     

    <div metal:define-macro="portlet_right"
         tal:omit-tag="" tal:define="portlets python:here.portlet_manager.getPortlets(here, 'columnTwo')">
        
      <tal:portlets tal:repeat="portlet portlets">
        <tal:block tal:define="macro_path portlet/getMacro;
                               macro python:path(macro_path)">
          <metal:macro use-macro="macro" />
        </tal:block>
      </tal:portlets>
    </div>  

</body>
</html>
