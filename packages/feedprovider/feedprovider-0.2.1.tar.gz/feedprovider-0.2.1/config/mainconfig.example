# feedprovider config
#
#
# TAKE NOTE THAT FORMAT IS IN JSON NOW SO USE " not '

## DOMAIN 

domain = "googlewave.com"

## APPLICATION NAME

appname = "FEEDPROVIDER"
app_id = "feedprovider"

## GLOBAL OWNER .. example: owner = ["~bart@127.0.0.1"] (include ident if needed)

owner = ["user@host"]

# enable debugging
debug = 0

# auto register new users 

auto_register = 1

