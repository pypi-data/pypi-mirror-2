<div id="ijabbar" tal:define="iJabConf view/iJabConf;
							  site_url view/site_url;">
	<tal:dummy define="random python:modules['random'];
					   dummy python:request.RESPONSE.setCookie('__ijab_name','visitor'+str(random.randint(1,1000)));
					   dummy python:request.RESPONSE.setCookie('__ijab_password','fake_passwd')" />
	
	<link type="text/css" rel="stylesheet"
		tal:attributes="href string:${site_url}/ijab/standard/iJab.css" />
	
	<script type="text/javascript"
		tal:content="string:var iJabConf=eval(${iJabConf});">
		var iJabConf = {...};
    </script>
    
	<script type="text/javascript" src="ijab.nocache.js"
    	tal:attributes="src string:${site_url}/ijab/ijab.nocache.js">
    </script>
</div>
