<?xml version="1.0"?>
<object name="portal_actions"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="folder_buttons">
    <property name="title"></property>
    
    <object name="paste_as_alias" meta_type="CMF Action"
            i18n:domain="plone">
      <property name="title" i18n:translate="">Paste as Alias</property>
      <property name="description" i18n:translate=""></property>
      <property name="url_expr">string:pasteAlias:method</property>
      <property name="icon_expr"></property>
      <property name="available_expr">folder/cb_dataValid</property>
      <property name="permissions">
        <element value="SimpleAlias: Add Alias"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
