  <!-- Get rid of list-definition below -->
  <define name="preamble">
    <element name="preamble">
      <ref name="preamble-attlist"/>
      <ref name="rtf-definition"/>
      <ref name="font-table"/>
      <ref name="color-table"/>
      <optional>
