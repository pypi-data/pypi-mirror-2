    </element>
  </define>
  <define name="preamble-attlist" combine="interleave">
    <empty/>
  </define>
  <define name="rtf-definition">
    <element name="rtf-definition">
      <ref name="rtf-definition-attlist"/>
      <empty/>
    </element>
  </define>
  <define name="rtf-definition-attlist" combine="interleave">
    <attribute name="default-font"/>
    <attribute name="code-page"/>
    <attribute name="platform"/>
  </define>
  <define name="font-table">
    <element name="font-table">
      <ref name="font-table-attlist"/>
      <oneOrMore>
        <ref name="font-in-table"/>
      </oneOrMore>
    </element>
  </define>
  <define name="font-table-attlist" combine="interleave">
