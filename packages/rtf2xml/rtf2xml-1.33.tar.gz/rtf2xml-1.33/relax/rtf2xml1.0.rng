    </element>
  </define>
  <define name="font-in-table-attlist" combine="interleave">
    <attribute name="num"/>
    <attribute name="name"/>
  </define>
  <define name="body">
    <element name="body">
      <ref name="body-attlist"/>
      <oneOrMore>
        <ref name="section"/>
      </oneOrMore>
    </element>
  </define>
  <define name="body-attlist" combine="interleave">
    <empty/>
  </define>
  <define name="section">
    <element name="section">
      <ref name="section-attlist"/>
      <zeroOrMore>
        <choice>
          <ref name="header-or-footer"/>
          <ref name="paragraph-definition"/>
          <ref name="style-group"/>
          <ref name="border-group"/>
          <ref name="list"/>
          <ref name="field-block"/>
          <ref name="table"/>
          <ref name="section"/>
          <ref name="page-break"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="section-attlist" combine="interleave">
    <attribute name="num"/>
