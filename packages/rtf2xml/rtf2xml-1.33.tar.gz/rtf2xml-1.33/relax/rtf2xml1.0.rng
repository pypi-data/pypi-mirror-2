    </optional>
    <optional>
      <attribute name="double-strike-through"/>
    </optional>
    <optional>
      <attribute name="emboss"/>
    </optional>
    <optional>
      <attribute name="engrave"/>
    </optional>
    <optional>
      <attribute name="font-color"/>
    </optional>
    <optional>
      <attribute name="font-size"/>
    </optional>
    <optional>
      <attribute name="font-style"/>
    </optional>
    <optional>
      <attribute name="footnote-marker">
        <choice>
          <value>true</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="hidden"/>
    </optional>
    <optional>
      <attribute name="italics">
        <choice>
          <value>true</value>
          <value>false</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="language"/>
    </optional>
    <optional>
      <attribute name="outline">
        <choice>
          <value>true</value>
          <value>false</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="shadow"/>
    </optional>
    <optional>
      <attribute name="small-caps">
        <choice>
          <value>true</value>
          <value>false</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="strike-through"/>
    </optional>
    <optional>
      <attribute name="subscript"/>
    </optional>
    <optional>
      <attribute name="superscript"/>
    </optional>
    <optional>
      <attribute name="underlined"/>
    </optional>
    <attribute name="style-number"/>
  </define>
  <define name="field-block">
    <element name="field-block">
      <ref name="field-block-attlist"/>
      <zeroOrMore>
        <choice>
          <ref name="field-block"/>
          <ref name="paragraph-definition"/>
          <ref name="style-group"/>
          <ref name="border-group"/>
          <ref name="list"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="field-block-attlist" combine="interleave">
    <ref name="field-att"/>
  </define>
  <define name="table">
    <element name="table">
      <ref name="table-attlist"/>
      <zeroOrMore>
        <choice>
          <ref name="row"/>
          <ref name="page-break"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="table-attlist" combine="interleave">
    <optional>
      <attribute name="average-cell-width"/>
    </optional>
    <optional>
      <attribute name="number-of-columns"/>
    </optional>
    <optional>
      <attribute name="average-cells-per-row"/>
    </optional>
    <optional>
      <attribute name="number-of-rows"/>
    </optional>
  </define>
  <define name="row">
    <element name="row">
      <ref name="row-attlist"/>
      <zeroOrMore>
        <ref name="cell"/>
      </zeroOrMore>
    </element>
  </define>
  <define name="row-attlist" combine="interleave">
    <ref name="row-attributes"/>
  </define>
  <define name="cell">
    <element name="cell">
      <ref name="cell-attlist"/>
      <zeroOrMore>
        <choice>
          <ref name="paragraph-definition"/>
          <ref name="style-group"/>
          <ref name="border-group"/>
          <ref name="list"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="cell-attlist" combine="interleave">
    <ref name="cell-attributes"/>
  </define>
  <define name="inline">
    <element name="inline">
      <ref name="inline-attlist"/>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="inline"/>
          <ref name="footnote"/>
          <ref name="anchor"/>
          <ref name="index-entry"/>
          <ref name="field"/>
          <ref name="udef_symbol"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="inline-attlist" combine="interleave">
    <ref name="char-styles"/>
  </define>
  <define name="pict">
    <element name="pict">
      <ref name="pict-attlist"/>
      <empty/>
    </element>
  </define>
  <define name="pict-attlist" combine="interleave">
    <attribute name="num"/>
  </define>
  <define name="list">
    <element name="list">
      <ref name="list-attlist"/>
      <zeroOrMore>
        <choice>
          <ref name="item"/>
          <ref name="page-break"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="list-attlist" combine="interleave">
    <attribute name="list-type">
      <choice>
        <value>ordered</value>
        <value>not-defined</value>
        <value>unordered</value>
      </choice>
    </attribute>
    <attribute name="list-id"/>
    <ref name="list-atts"/>
    <optional>
      <attribute name="list-hybrid">
        <choice>
          <value>true</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="list-template-id"/>
    </optional>
  </define>
  <define name="item">
    <element name="item">
      <ref name="item-attlist"/>
      <zeroOrMore>
        <choice>
          <ref name="paragraph-definition"/>
          <ref name="style-group"/>
          <ref name="border-group"/>
          <ref name="list"/>
          <ref name="page-break"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="item-attlist" combine="interleave">
    <empty/>
  </define>
  <define name="group-border" combine="choice">
    <notAllowed/>
  </define>
  <start>
    <choice>
      <ref name="field-result"/>
      <ref name="doc"/>
      <ref name="table-of-authorities"/>
      <ref name="section-definition"/>
      <ref name="index"/>
      <ref name="table-of-contents"/>
      <ref name="table-of-figures"/>
    </choice>
  </start>
</grammar>
