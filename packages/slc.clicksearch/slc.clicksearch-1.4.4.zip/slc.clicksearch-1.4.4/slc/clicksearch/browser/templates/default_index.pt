<ul tal:define="nums view/calc_num_results">
    <span tal:repeat="value view/valueSet">
      <li tal:define="num python:nums.get(value.get('id'), True)" tal:condition="num">
        <a tal:attributes="href value/link">
            <span tal:replace="value/title">Catalog Index Value</span>
            <span tal:condition="python:type(num)==int">(<span tal:replace="num">17</span>)</span>
         </a>
      </li>
    </span>
</ul>