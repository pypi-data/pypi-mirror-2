<?xml version="1.0"?>
<object name="portal_skins">

  <object name="plone_services_center" meta_type="Filesystem Directory View"
          directory="Products.PloneServicesCenter:skins/plone_services_center"/>

  <object name="plone_services_center_images" meta_type="Filesystem Directory View"
          directory="Products.PloneServicesCenter:skins/plone_services_center_images"/>

  <object name="plone_services_center_scripts" meta_type="Filesystem Directory View"
          directory="Products.PloneServicesCenter:skins/plone_services_center_scripts"/>

  <object name="plone_services_center_styles" meta_type="Filesystem Directory View"
          directory="Products.PloneServicesCenter:skins/plone_services_center_styles"/>
 
  <skin-path name="*">
    <layer name="plone_services_center" insert-after="custom"/>
    <layer name="plone_services_center_images" insert-after="custom"/>
    <layer name="plone_services_center_scripts" insert-after="custom"/>
    <layer name="plone_services_center_styles" insert-after="custom"/>
  </skin-path>

</object>
