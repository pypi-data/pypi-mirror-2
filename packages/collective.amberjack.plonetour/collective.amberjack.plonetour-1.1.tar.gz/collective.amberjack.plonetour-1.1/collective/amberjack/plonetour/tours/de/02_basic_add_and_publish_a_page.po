msgid "0_1_microstep_description"
msgstr ""

#. Default: "Add and publish a page"
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "0_add-and-publish-a-page_title"
msgstr "Eine Seite anlegen und veröffentlichen"

#. Default: "Click the [Add new...] drop-down menu."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "1_0_microstep_description"
msgstr "Klicken Sie auf [Hinzufügen...]"

#. Default: "Select [Page] from the menu."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "1_1_microstep_description"
msgstr "Wählen Sie [Seite] aus dem Menü."

#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "1_2_microstep_description"
msgstr ""

#. Default: "Add a page"
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "1_add-a-page_title"
msgstr "Seite anlegen"

#. Default: "Provide a [Title]: My Page."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_0_microstep_description"
msgstr "Geben Sie einen [Titel] ein: 'My Page'"

#. Default: "Provide a [Description]: This is my first Plone page."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_1_microstep_description"
msgstr "Geben Sie eine [Zusammenfassung] ein: 'Dies ist meine erste Seite'."

#. Default: "Add some page content in the [Body Text] field (you can come back and edit this later)."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_2_microstep_description"
msgstr "Geben Sie in das Feld [Haupttext] beliebigen Inhalt ein (Sie können diese Inhalte später jederzeit ändern)."

#. Default: "[Save] the page."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_3_microstep_description"
msgstr "[Speichern] Sie die Seite."

#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_4_microstep_description"
msgstr ""

#. Default: "Fill out the fields"
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_fill-out-the-fields_title"
msgstr "Felder ausfüllen"

#. Default: "Now that you selected the Page content type, you need to supply some information about it."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "2_fill-out-the-fields_text"
msgstr "Nachdem Sie den Typ Seite ausgewählt haben müssen Sie diesen mit Inhalten füllen."

#. Default: "Click the [State] drop-down menu."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "3_0_microstep_description"
msgstr "Klicken Sie auf [Status]."

#. Default: "Select [Publish] from the menu."
#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "3_1_microstep_description"
msgstr "Wählen Sie [Veröffentlichen] aus dem Menü."

#: /home/zope/virtualenv/i18ndude/tours/02_basic_add_and_publish_a_page.cfg
msgid "3_2_microstep_description"
msgstr ""

