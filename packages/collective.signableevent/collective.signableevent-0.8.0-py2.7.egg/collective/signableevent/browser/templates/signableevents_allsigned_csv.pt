<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">
    
    <metal:override fill-slot="portlets_one_slot">
      <metal:prefs use-macro="here/portlet_prefs/macros/portlet" />
    </metal:override>

    <metal:override fill-slot="column_two_slot" />

  <body>
    <div metal:fill-slot="content"
         id="content">

      <h1 i18n:translate="export_for_all_signers">Export for all signers to csv</h1>
      <form action="signableevents_allsigned_exportcsv"
            class="enableUnloadProtection"
            method="post">
        <br />
        <input type="submit" name="exportAllSignersToCsv" value="Export signers to csv" 
            i18n:attributes="value export_signers_to_csv" />

      </form>
    </div>

  </body>
</html>
