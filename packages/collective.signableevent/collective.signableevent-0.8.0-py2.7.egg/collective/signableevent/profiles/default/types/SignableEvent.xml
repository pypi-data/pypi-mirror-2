<?xml version="1.0"?>
<object name="SignableEvent"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="collective.signableevent" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">SignableEvent</property>
  <property name="description"
    i18n:translate="">An event to which people may sign up.</property>
  <property name="content_meta_type">SignableEvent</property>
  <property name="icon_expr">string:$portal_url/signable_event_icon.png</property>
  <property name="product">collective.signableevent</property>
  <property name="factory">addSignableEvent</property>
  <property name="immediate_view">signable_event_view</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">True</property>
  <property name="allowed_content_types">
   <element value="EventSignup"/>
  </property>
  <property name="allow_discussion">False</property>
  <property name="default_view">signable_event_view</property>
  <property name="view_methods">
    <element value="signable_event_view" />
  </property>
  <alias from="(Default)" to="(dynamic view)" />
  <alias from="edit" to="atct_edit" />
  <alias from="sharing" to="@@sharing" />
  <alias from="view" to="(selected layout)" />
  <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${folder_url}/" visible="True">
    <permission value="View" />
  </action>
  <action title="Edit" action_id="edit" category="object"
    condition_expr="not:object/@@plone_lock_info/is_locked_for_current_user"
    icon_expr="" link_target="" url_expr="string:${object_url}/edit"
    visible="True">
  <permission value="Modify portal content"/>
  </action>
  <action title="Properties" action_id="metadata" category="object"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${object_url}/properties" visible="True">
  <permission value="Modify portal content"/>
  </action>
  <action title="iCalendar" action_id="ics" category="document_actions"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${object_url}/ics_view" visible="True">
  <permission value="View"/>
  </action>
  <action title="vCalendar" action_id="vcs" category="document_actions"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${object_url}/vcs_view" visible="True">
  <permission value="View"/>
  </action>
  <action title="View signers" action_id="signers" category="object"
    condition_expr="" icon_expr="" link_target=""
    url_expr="string:${object_url}/signable_event_signers" visible="True">
  <permission value="List folder contents"/>
 </action>
</object>
