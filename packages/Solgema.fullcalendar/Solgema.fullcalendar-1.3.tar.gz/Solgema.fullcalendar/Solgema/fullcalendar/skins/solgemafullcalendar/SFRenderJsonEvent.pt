<html>
<head>
<script type="text/javascript" src="./jquery.js"></script>
<script type="text/javascript" src="./jquery-integration.js"></script>
</head>
<body>
<tal:iefixstart replace="structure string:&lt;script&gt;" />
  var last_referer = '<p tal:replace="request/last_referer"/>';
<tal:iefixstart replace="structure string:&lt;/script&gt;" />
<script>
  var $dialogContent = window.parent.jq('#event_edit_container');
  var $calendar = window.parent.jq('#calendar');
  var $kssspinner = window.parent.jq('#kss-spinner');
  jq.ajax({
    type :   'POST',
    url :    './SFJsonEvent',
    dataType: "json",
    data :   {'last_referer':last_referer},
    async:   false,
    success :function(json) {
      $calendar.fullCalendar('removeEvents', [json['id'],]);
      $calendar.fullCalendar('renderEvent', json, false);
      $dialogContent.dialog('close');
      $kssspinner.hide();
    }
  });
</script>
</body>
</html>
