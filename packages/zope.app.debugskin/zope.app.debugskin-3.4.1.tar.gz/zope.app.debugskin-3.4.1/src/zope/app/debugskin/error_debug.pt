<html metal:use-macro="context/@@standard_macros/dialog"
    i18n:domain="zope">
<body>
<div metal:fill-slot="body">

  <h3 i18n:translate="">
    Error type: 
    <tal:span tal:replace="nocall:view/error_type" i18n:name="error_type"/>
  </h3>

  <h5 i18n:translate="">
    Error object: 
    <tal:span tal:replace="nocall:view/error_object" i18n:name="error_object"/>
  </h5>

  <pre class="traceback" style="font-size:small;">
    <tal:line tal:repeat="line view/traceback_lines"
              tal:replace="line">TRACEBACK LINE</tal:line>
  </pre>

</div>
</body>
</html>
