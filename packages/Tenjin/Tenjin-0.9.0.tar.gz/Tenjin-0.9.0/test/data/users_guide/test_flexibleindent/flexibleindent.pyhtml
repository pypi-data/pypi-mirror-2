<html>
  <body>
    <div id="main-content">
      <?py if items: ?>
      <table>
        <tbody>
          <?py i = 0 ?>
          <?py for item in items: ?>
          <?py   i += 1 ?>
          <tr>
            <td>${i}</td>
            <?py if item: ?>
            <td>${item}</td>
            <?py else: ?>
            <td>-</td>
            <?py #endif ?>
          </tr>
        <?py #endfor ?>
        </tbody>
      </table>
      <?py #endif ?>
    </div>
  </body>
</html>
