
options()

depends(
    docs ['docs_build' 'docs_upload']
)

targets(
    docs_build [
        $ rm -f docs/build/doctrees/environment.pickle
        $ export PYTHONPATH=`pwd` && cd docs && make html && cd -
    ]
    docs_upload
        $ scp -r docs/build/html/* glashammer.org:/var/www/glashammer.org/fom/

)
