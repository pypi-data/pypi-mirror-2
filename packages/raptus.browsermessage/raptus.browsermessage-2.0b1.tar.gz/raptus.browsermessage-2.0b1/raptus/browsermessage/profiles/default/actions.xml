<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="browsermessage_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="firefox" meta_type="CMF Action" i18n:domain="raptus.browsermessage">
   <property name="title" i18n:translate="">Firefox</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:http://getfirefox.com</property>
   <property name="icon_expr">string:firefox.png</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="chrome" meta_type="CMF Action" i18n:domain="raptus.browsermessage">
   <property name="title" i18n:translate="">Chrome</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:http://chrome.google.com</property>
   <property name="icon_expr">string:chrome.png</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="safari" meta_type="CMF Action" i18n:domain="raptus.browsermessage">
   <property name="title" i18n:translate="">Safari</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:http://apple.com/safari/</property>
   <property name="icon_expr">string:safari.png</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="opera" meta_type="CMF Action" i18n:domain="raptus.browsermessage">
   <property name="title" i18n:translate="">Opera</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:http://opera.com/browser/download/</property>
   <property name="icon_expr">string:opera.png</property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>