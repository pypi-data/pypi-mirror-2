<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="EasyNewsletter">
<body>

<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main"
           tal:define="kssClassesView context/@@kss_field_decorator_view;
                       getKssClasses nocall:kssClassesView/getKssClassesInlineEditable;
                       templateId template/getId;
                       text here/getText;">

        <div tal:replace="structure provider:plone.abovecontenttitle" />

        <h1 class="documentFirstHeading">
            <span tal:omit-tag=""
                  i18n:translate="easynewsletter_subcribers_label">Subscribers</span>
        </h1>

        <div tal:define="subscribers python:view.create_subscribers('dummy');
                         fail subscribers/fail;
                         success subscribers/success">
            
            <h3 i18n:translate="not_created_subscribers">Not imported subscribers</h3>
            <p tal:repeat="mail fail" tal:content="mail"></p>
            <h3 i18n:translate="sucessfully_created_subscribers">Sucessfully created Subscribers</h3>
            <p tal:repeat="mail success" tal:content="mail"></p>

        </div>

    </tal:main-macro>
</metal:main>

</body>
</html>
