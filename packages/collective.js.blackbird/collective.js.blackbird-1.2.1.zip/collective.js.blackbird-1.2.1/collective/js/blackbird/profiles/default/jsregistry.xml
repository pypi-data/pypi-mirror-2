<?xml version="1.0"?>
<object name="portal_javascripts" meta_type="JavaScripts Registry">

    <javascript 
        cacheable="True" 
        compression="safe" 
        cookable="True"
        enabled="1" 
        expression="python: here.restrictedTraverse('@@kss_devel_mode').ison()"
        id="++resource++blackbird.js"
        insert-after="jquery.js"
        />

    <javascript 
        cacheable="True" 
        compression="safe" 
        cookable="True"
        enabled="1" 
        expression="python: here.restrictedTraverse('@@kss_devel_mode').isoff()"
        id="++resource++blackbird_disabled.js"
        insert-after="jquery.js"
        />

</object>
